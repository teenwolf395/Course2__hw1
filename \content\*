        "\n",
        "    # Добавляем вариативности (синонимы и разные формулировки)\n",
        "    extended_intents = {}\n",
        "    for intent_name, intent_data in intents.items():\n",
        "        extended_patterns = []\n",
        "\n",
        "        for pattern in intent_data[\"patterns\"]:\n",
        "            # Добавляем оригинальный паттерн\n",
        "            extended_patterns.append(pattern)\n",
        "\n",
        "            # Добавляем с восклицательными знаками\n",
        "            extended_patterns.append(pattern + \"!\")\n",
        "            extended_patterns.append(pattern.upper())\n",
        "\n",
        "            # Добавляем с вопросительными знаками\n",
        "            if \"?\" not in pattern:\n",
        "                extended_patterns.append(pattern + \"?\")\n",
        "\n",
        "            # Добавляем с многоточием\n",
        "            extended_patterns.append(pattern + \"...\")\n",
        "\n",
        "            # Добавляем с эмоциями\n",
        "            extended_patterns.append(pattern + \" пожалуйста\")\n",
        "            extended_patterns.append(\"пожалуйста \" + pattern)\n",
        "            extended_patterns.append(\"можно \" + pattern)\n",
        "            extended_patterns.append(\"хочу \" + pattern)\n",
        "\n",
        "        extended_intents[intent_name] = {\n",
        "            \"patterns\": list(set(extended_patterns)),  # Убираем дубли\n",
        "            \"responses\": intent_data[\"responses\"]\n",
        "        }\n",
        "\n",
        "    # Сохраняем в файл\n",
        "    filename = 'extended_intents.json'\n",
        "    with open(filename, 'w', encoding='utf-8') as f:\n",
        "        json.dump(extended_intents, f, ensure_ascii=False, indent=2, sort_keys=True)\n",
        "\n",
        "    # Статистика\n",
        "    total_patterns = sum(len(data[\"patterns\"]) for data in extended_intents.values())\n",
        "    total_responses = sum(len(data[\"responses\"]) for data in extended_intents.values())\n",
        "\n",
        "    print(f\"Создан файл {filename}\")\n",
        "    print(f\"Статистика:\")\n",
        "    print(f\"   • Намерений: {len(extended_intents)}\")\n",
        "    print(f\"   • Паттернов: {total_patterns}\")\n",
        "    print(f\"   • Ответов: {total_responses}\")\n",
        "    print(f\"   • Среднее паттернов на намерение: {total_patterns/len(extended_intents):.1f}\")\n",
        "\n",
        "    # Показываем примеры\n",
        "    print(f\"\\nПримеры намерений:\")\n",
        "    for intent_name in list(extended_intents.keys())[:3]:\n",
        "        patterns = extended_intents[intent_name][\"patterns\"][:3]\n",
        "        print(f\"   • {intent_name}: {patterns}\")\n",
        "\n",
        "    return extended_intents\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    print(\"=\" * 70)\n",
        "    print(\"СОЗДАНИЕ РАСШИРЕННОГО НАБОРА ДАННЫХ ДЛЯ ОБУЧЕНИЯ НЕЙРОСЕТИ\")\n",
        "    print(\"=\" * 70)\n",
        "\n",
        "    intents = create_extended_intents_file()\n",
        "    print(\"\\nДанные для обучения готовы!\")"
      ]
    },
