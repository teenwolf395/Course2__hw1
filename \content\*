      "cell_type": "code",
      "source": [
        "import os\n",
        "import json\n",
        "import re\n",
        "import random\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "from collections import Counter\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.preprocessing import LabelEncoder\n",
        "from sklearn.metrics import classification_report, confusion_matrix\n",
        "import torch\n",
        "import torch.nn as nn\n",
        "import torch.nn.functional as F\n",
        "import torch.optim as optim\n",
        "from torch.utils.data import Dataset, DataLoader\n",
        "import torch.optim.lr_scheduler as lr_scheduler\n",
        "import logging\n",
        "from datetime import datetime\n",
        "import sys\n",
        "\n",
        "print(\"=\" * 70)\n",
        "print(\"ОБУЧЕНИЕ НЕЙРОСЕТИ ДЛЯ КЛАССИФИКАЦИИ НАМЕРЕНИЙ\")\n",
        "print(\"=\" * 70)\n",
        "\n",
        "# ============================================\n",
        "# НАСТРОЙКА ЛОГИРОВАНИЯ\n",
        "# ============================================\n",
        "\n",
        "def setup_logging(log_level=logging.INFO, log_to_file=True, log_filename=None):\n",
        "    \"\"\"\n",
        "    Настройка системы логирования\n",
        "\n",
        "    Args:\n",
        "        log_level: Уровень логирования (DEBUG, INFO, WARNING, ERROR, CRITICAL)\n",
        "        log_to_file: Записывать ли логи в файл\n",
        "        log_filename: Имя файла для логов (если None, генерируется автоматически)\n",
        "    \"\"\"\n",
        "    # Создаем директорию для логов, если она не существует\n",
        "    log_dir = \"training_logs\"\n",
        "    if not os.path.exists(log_dir):\n",
        "        os.makedirs(log_dir)\n",
        "\n",
        "    # Генерируем имя файла логов\n",
        "    if log_filename is None:\n",
        "        timestamp = datetime.now().strftime(\"%Y%m%d_%H%M%S\")\n",
        "        log_filename = f\"{log_dir}/training_{timestamp}.log\"\n",
        "    else:\n",
        "        log_filename = f\"{log_dir}/{log_filename}\"\n",
        "\n",
        "    # Создаем логгер\n",
        "    logger = logging.getLogger('IntentClassifier')\n",
        "    logger.setLevel(log_level)\n",
        "\n",
        "    # Удаляем старые обработчики, если они есть\n",
        "    logger.handlers.clear()\n",
        "\n",
        "    # Форматтер для логов\n",
        "    formatter = logging.Formatter(\n",
        "        '%(asctime)s - %(name)s - %(levelname)s - %(message)s',\n",
        "        datefmt='%Y-%m-%d %H:%M:%S'\n",
        "    )\n",
        "\n",
        "    # Обработчик для вывода в консоль\n",
        "    console_handler = logging.StreamHandler(sys.stdout)\n",
        "    console_handler.setLevel(log_level)\n",
        "    console_handler.setFormatter(formatter)\n",
        "    logger.addHandler(console_handler)\n",
        "\n",
        "    # Обработчик для записи в файл\n",
        "    if log_to_file:\n",
        "        file_handler = logging.FileHandler(log_filename, encoding='utf-8')\n",
        "        file_handler.setLevel(log_level)\n",
        "        file_handler.setFormatter(formatter)\n",
        "        logger.addHandler(file_handler)\n",
        "\n",
        "    # Логгер для прогресса обучения\n",
        "    progress_logger = logging.getLogger('TrainingProgress')\n",
        "    progress_logger.setLevel(logging.INFO)\n",
        "\n",
        "    if log_to_file:\n",
        "        progress_handler = logging.FileHandler(f\"{log_dir}/progress_{timestamp}.log\", encoding='utf-8')\n",
        "        progress_handler.setLevel(logging.INFO)\n",
        "        progress_formatter = logging.Formatter('%(asctime)s - %(message)s', datefmt='%H:%M:%S')\n",
        "        progress_handler.setFormatter(progress_formatter)\n",
        "        progress_logger.addHandler(progress_handler)\n",
        "\n",
        "    logger.info(f\"Логирование настроено. Логи будут записываться в: {log_filename}\")\n",
        "    return logger, progress_logger, log_filename\n",
        "\n",
        "# Инициализация логгера\n",
        "logger, progress_logger, current_log_file = setup_logging(log_level=logging.INFO, log_to_file=True)\n",
        "\n",
        "# ============================================\n",
        "# КЛАСС ДЛЯ ДАННЫХ\n",
        "# ============================================\n",
        "\n",
        "class IntentDataset(Dataset):\n",
        "    def __init__(self, texts, labels, vocab, max_len=50):\n",
        "        self.texts = texts\n",
        "        self.labels = labels\n",
        "        self.vocab = vocab\n",
        "        self.max_len = max_len\n",
        "\n",
        "    def __len__(self):\n",
        "        return len(self.texts)\n",
        "\n",
        "    def __getitem__(self, idx):\n",
        "        text = self.texts[idx]\n",
        "        label = self.labels[idx]\n",
        "\n",
        "        # Токенизация\n",
        "        tokens = re.findall(r'\\b\\w+\\b', text.lower())\n",
        "        indices = [self.vocab.get(token, 1) for token in tokens]\n",
        "\n",
        "        # Ограничение длины\n",
        "        if len(indices) > self.max_len:\n",
        "            indices = indices[:self.max_len]\n",
        "            length = self.max_len\n",
        "        else:\n",
        "            length = len(indices)\n",
        "            indices += [0] * (self.max_len - len(indices))\n",
        "\n",
        "        return {\n",
        "            'text': torch.tensor(indices, dtype=torch.long),\n",
        "            'label': torch.tensor(label, dtype=torch.long),\n",
        "            'length': length\n",
        "        }\n",
        "\n",
        "# ============================================\n",
        "# АРХИТЕКТУРА НЕЙРОСЕТИ\n",
        "# ============================================\n",
        "\n",
        "class EnhancedIntentClassifier(nn.Module):\n",
        "    def __init__(self, vocab_size, embedding_dim=128, hidden_dim=256,\n",
        "                 output_dim=8, num_layers=2, dropout=0.3):\n",
        "        super().__init__()\n",
        "\n",
        "        # Embedding слой\n",
        "        self.embedding = nn.Embedding(vocab_size, embedding_dim, padding_idx=0)\n",
        "        self.embedding_dropout = nn.Dropout(dropout)\n",
        "\n",
        "        # BiLSTM\n",
        "        self.lstm = nn.LSTM(\n",
        "            embedding_dim,\n",
        "            hidden_dim,\n",
        "            num_layers=num_layers,\n",
        "            bidirectional=True,\n",
        "            batch_first=True,\n",
        "            dropout=dropout if num_layers > 1 else 0\n",
        "        )\n",
        "\n",
        "        # Attention механизм\n",
        "        self.attention = nn.Sequential(\n",
        "            nn.Linear(hidden_dim * 2, hidden_dim),\n",
        "            nn.Tanh(),\n",
        "            nn.Linear(hidden_dim, 1)\n",
        "        )\n",
        "\n",
        "        # Полносвязные слои\n",
        "        self.fc_layers = nn.Sequential(\n",
        "            nn.Linear(hidden_dim * 2, hidden_dim),\n",
        "            nn.BatchNorm1d(hidden_dim),\n",
        "            nn.ReLU(),\n",
        "            nn.Dropout(dropout),\n",
        "            nn.Linear(hidden_dim, hidden_dim // 2),\n",
        "            nn.BatchNorm1d(hidden_dim // 2),\n",
        "            nn.ReLU(),\n",
        "            nn.Dropout(dropout),\n",
        "            nn.Linear(hidden_dim // 2, output_dim)\n",
        "        )\n",
        "\n",
        "        # Инициализация весов\n",
        "        self._init_weights()\n",
        "\n",
        "    def _init_weights(self):\n",
        "        \"\"\"Инициализация весов\"\"\"\n",
        "        for name, param in self.named_parameters():\n",
        "            if 'bias' in name:\n",
        "                nn.init.constant_(param.data, 0.0)\n",
        "            elif 'weight' in name and param.dim() >= 2:\n",
        "                if 'lstm' in name:\n",
        "                    nn.init.orthogonal_(param.data)\n",
        "                elif 'embedding' in name:\n",
        "                    nn.init.normal_(param.data, mean=0, std=0.1)\n",
        "                elif 'attention' in name or 'fc' in name or 'linear' in name:\n",
        "                    nn.init.xavier_uniform_(param.data)\n",
        "\n",
        "    def forward(self, text, lengths=None):\n",
        "        # Embedding\n",
        "        embedded = self.embedding(text)\n",
        "        embedded = self.embedding_dropout(embedded)\n",
        "\n",
        "        # Packed sequence для LSTM\n",
        "        if lengths is not None:\n",
        "            packed_input = nn.utils.rnn.pack_padded_sequence(\n",
        "                embedded, lengths.cpu(), batch_first=True, enforce_sorted=False\n",
        "            )\n",
        "            packed_output, (hidden, cell) = self.lstm(packed_input)\n",
        "            output, _ = nn.utils.rnn.pad_packed_sequence(packed_output, batch_first=True)\n",
        "        else:\n",
        "            output, (hidden, cell) = self.lstm(embedded)\n",
        "\n",
        "        # Attention\n",
        "        attention_weights = F.softmax(self.attention(output), dim=1)\n",
        "        context = torch.sum(attention_weights * output, dim=1)\n",
        "\n",
        "        # Классификация\n",
        "        logits = self.fc_layers(context)\n",
        "\n",
        "        return logits\n",
        "\n",
        "# ============================================\n",
        "# ФУНКЦИИ ДЛЯ ОБУЧЕНИЯ\n",
        "# ============================================\n",
        "\n",
        "def load_intents_data(filename='extended_intents.json'):\n",
        "    \"\"\"Загрузка данных из файла\"\"\"\n",
        "    logger.info(f\"Загрузка данных из {filename}...\")\n",
        "\n",
        "    if not os.path.exists(filename):\n",
        "        logger.error(f\"Файл {filename} не найден!\")\n",
        "        logger.info(\"Создайте файл с помощью create_intents_data.py\")\n",
        "        return None, None, None, None\n",
        "\n",
        "    try:\n",
        "        with open(filename, 'r', encoding='utf-8') as f:\n",
        "            intents = json.load(f)\n",
        "    except Exception as e:\n",
        "        logger.error(f\"Ошибка загрузки файла: {e}\")\n",
        "        return None, None, None, None\n",
        "\n",
        "    # Подготовка данных\n",
        "    texts = []\n",
        "    labels = []\n",
        "\n",
        "    for intent_name, intent_data in intents.items():\n",
        "        patterns = intent_data.get('patterns', [])\n",
        "        for pattern in patterns:\n",
        "            texts.append(pattern)\n",
        "            labels.append(intent_name)\n",
        "\n",
        "    logger.info(f\"Загружено {len(texts)} примеров, {len(intents)} классов\")\n",
        "    logger.info(f\"Классы: {list(intents.keys())}\")\n",
        "\n",
        "    return texts, labels, list(intents.keys())\n",
        "\n",
        "def create_vocab(texts, min_freq=2):\n",
        "    \"\"\"Создание словаря\"\"\"\n",
        "    logger.info(\"Создание словаря...\")\n",
        "\n",
        "    word_counts = Counter()\n",
        "    for text in texts:\n",
        "        tokens = re.findall(r'\\b\\w+\\b', text.lower())\n",
        "        word_counts.update(tokens)\n",
        "\n",
        "    vocab = {'<PAD>': 0, '<UNK>': 1}\n",
        "    for word, count in word_counts.items():\n",
        "        if count >= min_freq:\n",
        "            vocab[word] = len(vocab)\n",
        "\n",
        "    logger.info(f\"Всего слов: {len(word_counts)}\")\n",
        "    logger.info(f\"Словарь: {len(vocab)} слов (частота >= {min_freq})\")\n",
        "\n",
        "    # Дополнительная информация о словаре\n",
        "    logger.debug(f\"Примеры слов в словаре: {list(vocab.keys())[:10]}...\")\n",
        "\n",
        "    return vocab\n",
        "\n",
        "def train_model(model, train_loader, val_loader, vocab, label_encoder, num_epochs=20, device='cpu'):\n",
        "    \"\"\"Обучение модели\"\"\"\n",
        "    logger.info(\"Начало обучения...\")\n",
        "\n",
        "    # Создаем файл для детального лога обучения\n",
        "    timestamp = datetime.now().strftime(\"%Y%m%d_%H%M%S\")\n",
        "    detailed_log_file = f\"training_logs/detailed_training_{timestamp}.csv\"\n",
        "\n",
        "    # Подготовка заголовков для детального лога\n",
        "    with open(detailed_log_file, 'w', encoding='utf-8') as f:\n",
        "        f.write(\"epoch,train_loss,train_accuracy,val_accuracy,learning_rate,batch_loss\\n\")\n",
        "\n",
        "    criterion = nn.CrossEntropyLoss()\n",
        "    optimizer = optim.AdamW(model.parameters(), lr=0.00001, weight_decay=1e-4)\n",
        "\n",
        "    # Создание scheduler\n",
        "    try:\n",
        "        scheduler = lr_scheduler.ReduceLROnPlateau(\n",
        "            optimizer, mode='max', patience=3, factor=0.5, verbose=True\n",
        "        )\n",
        "    except TypeError:\n",
        "        scheduler = lr_scheduler.ReduceLROnPlateau(\n",
        "            optimizer, mode='max', patience=3, factor=0.5\n",
        "        )\n",
        "        logger.warning(\"Используется старая версия PyTorch\")\n",
        "\n",
        "    train_losses = []\n",
        "    train_accuracies = []\n",
        "    val_accuracies = []\n",
        "    best_accuracy = 0\n",
        "\n",
        "    # Информация о начале обучения\n",
        "    logger.info(f\"Параметры обучения:\")\n",
        "    logger.info(f\"  • Количество эпох: {num_epochs}\")\n",
        "    logger.info(f\"  • Размер батча: {train_loader.batch_size}\")\n",
        "    logger.info(f\"  • Размер обучающей выборки: {len(train_loader.dataset)}\")\n",
        "    logger.info(f\"  • Размер валидационной выборки: {len(val_loader.dataset)}\")\n",
        "    logger.info(f\"  • Устройство: {device}\")\n",
        "    logger.info(f\"  • Детальный лог сохраняется в: {detailed_log_file}\")\n",
        "\n",
        "    for epoch in range(num_epochs):\n",
        "        # Обучение\n",
        "        model.train()\n",
        "        total_loss = 0\n",
        "        correct = 0\n",
        "        total = 0\n",
        "\n",
        "        batch_counter = 0\n",
        "\n",
        "        for batch in train_loader:\n",
        "            texts = batch['text'].to(device)\n",
        "            labels = batch['label'].to(device)\n",
        "            lengths = batch['length'].to(device)\n",
        "\n",
        "            optimizer.zero_grad()\n",
        "            outputs = model(texts, lengths)\n",
        "            loss = criterion(outputs, labels)\n",
        "\n",
        "            loss.backward()\n",
        "            torch.nn.utils.clip_grad_norm_(model.parameters(), 1.0)\n",
        "            optimizer.step()\n",
        "\n",
        "            total_loss += loss.item()\n",
        "            _, predicted = torch.max(outputs, 1)\n",
        "            correct += (predicted == labels).sum().item()\n",
        "            total += labels.size(0)\n",
        "\n",
        "            # Логирование каждого батча (раз в 10 батчей или в конце эпохи)\n",
        "            batch_counter += 1\n",
        "            if batch_counter % 10 == 0 or batch_counter == len(train_loader):\n",
        "                batch_accuracy = (predicted == labels).sum().item() / labels.size(0)\n",
        "                progress_logger.info(f\"Эпоха {epoch+1}/{num_epochs} | \"\n",
        "                                    f\"Батч {batch_counter}/{len(train_loader)} | \"\n",
        "                                    f\"Loss: {loss.item():.4f} | \"\n",
        "                                    f\"Accuracy: {batch_accuracy:.2%}\")\n",
        "\n",
        "            # Запись детального лога для каждого батча\n",
        "            with open(detailed_log_file, 'a', encoding='utf-8') as f:\n",
        "                f.write(f\"{epoch+1},,,,{optimizer.param_groups[0]['lr']},{loss.item()}\\n\")\n",
        "\n",
        "        train_loss = total_loss / len(train_loader)\n",
        "        train_acc = correct / total\n",
        "\n",
        "        # Валидация\n",
        "        model.eval()\n",
        "        val_acc = evaluate_model(model, val_loader, device)\n",
        "\n",
        "        # Обновление learning rate\n",
        "        scheduler.step(val_acc)\n",
        "\n",
        "        # Логирование результатов эпохи\n",
        "        train_losses.append(train_loss)\n",
        "        train_accuracies.append(train_acc)\n",
        "        val_accuracies.append(val_acc)\n",
        "\n",
        "        # Текущий learning rate\n",
        "        current_lr = optimizer.param_groups[0]['lr']\n",
        "\n",
        "        # Запись результатов эпохи в детальный лог\n",
        "        with open(detailed_log_file, 'a', encoding='utf-8') as f:\n",
        "            f.write(f\"{epoch+1},{train_loss},{train_acc},{val_acc},{current_lr},\\n\")\n",
        "\n",
        "        # Логи в консоль и файл\n",
        "        epoch_log = (f\"Epoch {epoch+1:3d}/{num_epochs}: \"\n",
        "                     f\"Loss={train_loss:.4f}, \"\n",
        "                     f\"Train Acc={train_acc:.2%}, \"\n",
        "                     f\"Val Acc={val_acc:.2%}, \"\n",
        "                     f\"LR={current_lr:.6f}\")\n",
        "\n",
        "        if (epoch + 1) % 5 == 0 or epoch == 0:\n",
        "            logger.info(epoch_log)\n",
        "        else:\n",
        "            logger.debug(epoch_log)\n",
        "\n",
        "        # Сохранение лучшей модели\n",
        "        if val_acc > best_accuracy:\n",
        "            best_accuracy = val_acc\n",
        "            save_checkpoint(model, vocab, label_encoder.classes_, label_encoder, best_accuracy, epoch)\n",
        "            logger.info(f\"Новая лучшая модель сохранена! Точность: {best_accuracy:.2%}\")\n",
        "\n",
        "    # Финальное логирование\n",
        "    logger.info(f\"Обучение завершено!\")\n",
        "    logger.info(f\"  • Лучшая точность: {best_accuracy:.2%}\")\n",
        "    logger.info(f\"  • Финальная точность: {val_accuracies[-1]:.2%}\")\n",
        "    logger.info(f\"  • Детальный лог обучения: {detailed_log_file}\")\n",
        "\n",
        "    # Создание сводного отчета\n",
        "    create_training_summary(train_losses, train_accuracies, val_accuracies, num_epochs)\n",
        "\n",
        "    return train_losses, train_accuracies, val_accuracies\n",
        "\n",
        "def create_training_summary(train_losses, train_accuracies, val_accuracies, num_epochs):\n",
        "    \"\"\"Создание сводного отчета об обучении\"\"\"\n",
        "    summary_file = f\"training_logs/training_summary_{datetime.now().strftime('%Y%m%d_%H%M%S')}.txt\"\n",
        "\n",
        "    with open(summary_file, 'w', encoding='utf-8') as f:\n",
        "        f.write(\"=\" * 70 + \"\\n\")\n",
        "        f.write(\"СВОДНЫЙ ОТЧЕТ ОБ ОБУЧЕНИИ\\n\")\n",
        "        f.write(\"=\" * 70 + \"\\n\\n\")\n",
        "\n",
        "        f.write(f\"Общая информация:\\n\")\n",
        "        f.write(f\"  • Количество эпох: {num_epochs}\\n\")\n",
        "        f.write(f\"  • Лучшая точность на валидации: {max(val_accuracies):.2%}\\n\")\n",
        "        f.write(f\"  • Финальная точность на валидации: {val_accuracies[-1]:.2%}\\n\")\n",
        "        f.write(f\"  • Финальный loss: {train_losses[-1]:.4f}\\n\\n\")\n",
        "\n",
        "        f.write(\"Результаты по эпохам:\\n\")\n",
        "        f.write(\"Epoch | Train Loss | Train Acc | Val Acc | Improvement\\n\")\n",
        "        f.write(\"-\" * 70 + \"\\n\")\n",
        "\n",
        "        for epoch, (loss, train_acc, val_acc) in enumerate(zip(train_losses, train_accuracies, val_accuracies)):\n",
        "            improvement = \"\"\n",
        "            if epoch > 0:\n",
        "                improvement = f\"+{val_acc - val_accuracies[epoch-1]:+.2%}\"\n",
        "            f.write(f\"{epoch+1:5d} | {loss:10.4f} | {train_acc:9.2%} | {val_acc:8.2%} | {improvement:>12}\\n\")\n",
        "\n",
        "        f.write(\"\\n\" + \"=\" * 70 + \"\\n\")\n",
        "        f.write(\"АНАЛИЗ ПЕРЕОБУЧЕНИЯ:\\n\")\n",
        "        f.write(\"=\" * 70 + \"\\n\\n\")\n",
        "\n",
        "        if len(train_accuracies) == len(val_accuracies):\n",
        "            gaps = [train - val for train, val in zip(train_accuracies, val_accuracies)]\n",
        "            max_gap = max(gaps)\n",
        "            avg_gap = sum(gaps) / len(gaps)\n",
        "\n",
        "            f.write(f\"  • Максимальный разрыв между train и val: {max_gap:.2%}\\n\")\n",
        "            f.write(f\"  • Средний разрыв: {avg_gap:.2%}\\n\")\n",
        "\n",
        "            if max_gap > 0.3:\n",
        "                f.write(\"  ⚠️  ВНИМАНИЕ: Возможно переобучение!\\n\")\n",
        "            elif max_gap < 0.1:\n",
        "                f.write(\"  ✅ Модель хорошо обобщает\\n\")\n",
        "\n",
        "    logger.info(f\"Сводный отчет сохранен: {summary_file}\")\n",
        "    return summary_file\n",
        "\n",
        "def evaluate_model(model, data_loader, device='cpu'):\n",
        "    \"\"\"Оценка модели\"\"\"\n",
        "    model.eval()\n",
        "    correct = 0\n",
        "    total = 0\n",
        "\n",
        "    with torch.no_grad():\n",
        "        for batch in data_loader:\n",
        "            texts = batch['text'].to(device)\n",
        "            labels = batch['label'].to(device)\n",
        "            lengths = batch['length'].to(device)\n",
        "\n",
        "            outputs = model(texts, lengths)\n",
        "            _, predicted = torch.max(outputs, 1)\n",
        "            correct += (predicted == labels).sum().item()\n",
        "            total += labels.size(0)\n",
        "\n",
        "    accuracy = correct / total if total > 0 else 0\n",
        "    return accuracy\n",
        "\n",
        "def evaluate_full_model(model, val_loader, label_encoder, device='cpu'):\n",
        "    \"\"\"Полная оценка модели с метриками\"\"\"\n",
        "    logger.info(\"Полная оценка модели на валидационной выборке...\")\n",
        "\n",
        "    model.eval()\n",
        "    all_predictions = []\n",
        "    all_labels = []\n",
        "\n",
        "    with torch.no_grad():\n",
        "        for batch in val_loader:\n",
        "            texts = batch['text'].to(device)\n",
        "            labels = batch['label'].to(device)\n",
        "            lengths = batch['length'].to(device)\n",
        "\n",
        "            outputs = model(texts, lengths)\n",
        "            _, predicted = torch.max(outputs, 1)\n",
        "\n",
        "            all_predictions.extend(predicted.cpu().numpy())\n",
        "            all_labels.extend(labels.cpu().numpy())\n",
        "\n",
        "    # Преобразуем числовые метки в строковые\n",
        "    predicted_labels = label_encoder.inverse_transform(all_predictions)\n",
        "    true_labels = label_encoder.inverse_transform(all_labels)\n",
        "\n",
        "    # Генерация отчета о классификации\n",
        "    report = classification_report(true_labels, predicted_labels, zero_division=0, output_dict=True)\n",
        "\n",
        "    # Сохраняем отчет в файл\n",
        "    report_file = f\"training_logs/classification_report_{datetime.now().strftime('%Y%m%d_%H%M%S')}.json\"\n",
        "    with open(report_file, 'w', encoding='utf-8') as f:\n",
        "        json.dump(report, f, ensure_ascii=False, indent=2)\n",
        "\n",
        "    # Выводим отчет в лог\n",
        "    logger.info(\"\\nДетальный отчет о классификации:\")\n",
        "    report_str = classification_report(true_labels, predicted_labels, zero_division=0)\n",
        "    logger.info(f\"\\n{report_str}\")\n",
        "\n",
        "    # Сохраняем текстовый отчет\n",
        "    with open(report_file.replace('.json', '.txt'), 'w', encoding='utf-8') as f:\n",
        "        f.write(\"ОТЧЕТ О КЛАССИФИКАЦИИ\\n\")\n",
        "        f.write(\"=\" * 70 + \"\\n\\n\")\n",
        "        f.write(report_str)\n",
        "\n",
        "    logger.info(f\"Отчет о классификации сохранен: {report_file}\")\n",
        "\n",
        "    return all_predictions, all_labels\n",
        "\n",
        "def save_checkpoint(model, vocab, classes, label_encoder, accuracy, epoch):\n",
        "    \"\"\"Сохранение модели\"\"\"\n",
        "    checkpoint = {\n",
        "        'model_state_dict': model.state_dict(),\n",
        "        'vocab': vocab,\n",
        "        'classes': classes.tolist() if hasattr(classes, 'tolist') else classes,\n",
        "        'label_encoder_classes': label_encoder.classes_.tolist(),\n",
        "        'hyperparameters': {\n",
        "            'embedding_dim': model.embedding.embedding_dim,\n",
        "            'hidden_dim': model.lstm.hidden_size,\n",
        "            'num_layers': model.lstm.num_layers,\n",
        "            'dropout': 0.3\n",
        "        },\n",
        "        'accuracy': accuracy,\n",
        "        'epoch': epoch,\n",
        "        'timestamp': datetime.now().strftime(\"%Y-%m-%d %H:%M:%S\")\n",
        "    }\n",
        "\n",
        "    # Создаем директорию для чекпоинтов, если ее нет\n",
        "    checkpoints_dir = \"model_checkpoints\"\n",
        "    if not os.path.exists(checkpoints_dir):\n",
        "        os.makedirs(checkpoints_dir)\n",
        "\n",
        "    # Сохраняем с timestamp в имени файла\n",
        "    timestamp = datetime.now().strftime(\"%Y%m%d_%H%M%S\")\n",
        "    checkpoint_file = f\"{checkpoints_dir}/intent_classifier_{timestamp}.pth\"\n",
        "    torch.save(checkpoint, checkpoint_file)\n",
        "\n",
        "    # Также сохраняем как последнюю модель\n",
        "    torch.save(checkpoint, 'intent_classifier.pth')\n",
        "\n",
        "    logger.info(f\"Модель сохранена: {checkpoint_file}\")\n",
        "    logger.info(f\"  • Точность: {accuracy:.2%}\")\n",
        "    logger.info(f\"  • Эпоха: {epoch+1}\")\n",
        "\n",
        "    # Сохраняем информацию о чекпоинте в лог\n",
        "    with open(\"training_logs/checkpoints.log\", \"a\", encoding=\"utf-8\") as f:\n",
        "        f.write(f\"{timestamp},{epoch+1},{accuracy:.4f},{checkpoint_file}\\n\")\n",
        "\n",
        "def load_model_from_checkpoint(checkpoint_path='intent_classifier.pth', device='cpu'):\n",
        "    \"\"\"Загрузка модели из checkpoint\"\"\"\n",
        "    if not os.path.exists(checkpoint_path):\n",
        "        logger.error(f\"Файл {checkpoint_path} не найден\")\n",
        "        return None, None, None\n",
        "\n",
        "    try:\n",
        "        checkpoint = torch.load(checkpoint_path, map_location=device)\n",
        "        logger.info(f\"Модель загружена из {checkpoint_path}\")\n",
        "\n",
        "        # Создаем модель с теми же параметрами\n",
        "        model = EnhancedIntentClassifier(\n",
        "            vocab_size=len(checkpoint['vocab']),\n",
        "            embedding_dim=checkpoint['hyperparameters']['embedding_dim'],\n",
        "            hidden_dim=checkpoint['hyperparameters']['hidden_dim'],\n",
        "            output_dim=len(checkpoint['classes']),\n",
        "            num_layers=checkpoint['hyperparameters']['num_layers'],\n",
        "            dropout=checkpoint['hyperparameters']['dropout']\n",
        "        ).to(device)\n",
        "\n",
        "        # Загружаем веса\n",
        "        model.load_state_dict(checkpoint['model_state_dict'])\n",
        "        model.eval()\n",
        "\n",
        "        # Восстанавливаем label_encoder\n",
        "        label_encoder = LabelEncoder()\n",
        "        label_encoder.classes_ = np.array(checkpoint['label_encoder_classes'])\n",
        "\n",
        "        logger.info(f\"  • Точность при сохранении: {checkpoint['accuracy']:.2%}\")\n",
        "        logger.info(f\"  • Эпоха: {checkpoint['epoch'] + 1}\")\n",
        "        logger.info(f\"  • Время сохранения: {checkpoint.get('timestamp', 'неизвестно')}\")\n",
        "        logger.info(f\"  • Классы: {list(label_encoder.classes_)}\")\n",
        "\n",
        "        return model, checkpoint['vocab'], label_encoder\n",
        "\n",
        "    except Exception as e:\n",
        "        logger.error(f\"Ошибка при загрузке модели: {e}\")\n",
        "        return None, None, None\n",
        "\n",
        "def plot_training_results(train_losses, train_accs, val_accs):\n",
        "    \"\"\"Визуализация результатов обучения\"\"\"\n",
        "    if len(train_losses) == 0:\n",
        "        logger.warning(\"Нет данных для визуализации\")\n",
        "        return\n",
        "\n",
        "    fig, axes = plt.subplots(1, 3, figsize=(15, 4))\n",
        "\n",
        "    # Loss\n",
        "    axes[0].plot(train_losses, 'r-', linewidth=2)\n",
        "    axes[0].set_title('Loss во время обучения')\n",
        "    axes[0].set_xlabel('Эпоха')\n",
        "    axes[0].set_ylabel('Loss')\n",
        "    axes[0].grid(True, alpha=0.3)\n",
        "\n",
        "    # Accuracy\n",
        "    axes[1].plot(train_accs, 'b-', label='Train', linewidth=2)\n",
        "    axes[1].plot(val_accs, 'g-', label='Validation', linewidth=2)\n",
        "    axes[1].set_title('Accuracy во время обучения')\n",
        "    axes[1].set_xlabel('Эпоха')\n",
        "    axes[1].set_ylabel('Accuracy')\n",
        "    axes[1].legend()\n",
        "    axes[1].grid(True, alpha=0.3)\n",
        "\n",
        "    # Accuracy difference\n",
        "    if len(train_accs) == len(val_accs):\n",
        "        gap = [train - val for train, val in zip(train_accs, val_accs)]\n",
        "        axes[2].plot(gap, 'purple', linewidth=2, alpha=0.7)\n",
        "        axes[2].axhline(y=0, color='gray', linestyle='--', alpha=0.5)\n",
        "        axes[2].set_title('Разница между Train и Validation')\n",
        "        axes[2].set_xlabel('Эпоха')\n",
        "        axes[2].set_ylabel('Gap')\n",
        "        axes[2].grid(True, alpha=0.3)\n",
        "\n",
        "    plt.tight_layout()\n",
        "\n",
        "    # Сохраняем график\n",
        "    plot_file = f\"training_logs/training_plot_{datetime.now().strftime('%Y%m%d_%H%M%S')}.png\"\n",
        "    plt.savefig(plot_file, dpi=150, bbox_inches='tight')\n",
        "    plt.savefig('training_results.png', dpi=150, bbox_inches='tight')\n",
        "\n",
        "    logger.info(f\"Графики обучения сохранены: {plot_file}\")\n",
        "    plt.show()\n",
        "\n",
        "def test_model_on_examples(model, vocab, label_encoder, device='cpu'):\n",
        "    \"\"\"Тестирование модели на примерах\"\"\"\n",
        "    logger.info(\"Тестирование модели на конкретных примерах...\")\n",
        "\n",
        "    # Получаем маппинг числовых индексов на имена классов\n",
        "    class_names = label_encoder.classes_\n",
        "\n",
        "    logger.info(f\"Доступные классы: {list(class_names)}\")\n",
        "\n",
        "    # Тестовые примеры - должны совпадать с названиями классов из файла\n",
        "    test_cases = [\n",
        "        (\"привет как дела\", \"greeting\"),\n",
        "        (\"привет\", \"greeting\"),\n",
        "        (\"здравствуйте\", \"greeting\"),\n",
        "        (\"начать квиз\", \"start_quiz\"),\n",
        "        (\"хочу начать викторину\", \"start_quiz\"),\n",
        "        (\"давай сыграем в квиз\", \"start_quiz\"),\n",
        "        (\"хочу легкий уровень\", \"difficulty\"),\n",
        "        (\"сложный уровень пожалуйста\", \"difficulty\"),\n",
        "        (\"покажи статистику\", \"statistics\"),\n",
        "        (\"хочу увидеть мою статистику\", \"statistics\"),\n",
        "        (\"помоги мне\", \"help\"),\n",
        "        (\"мне нужна помощь\", \"help\"),\n",
        "        (\"пока\", \"goodbye\"),\n",
        "        (\"до свидания\", \"goodbye\"),\n",
        "        (\"правильно\", \"answer_feedback\"),\n",
        "        (\"неправильно\", \"answer_feedback\"),\n",
        "        (\"еще раз\", \"repeat_quiz\"),\n",
        "        (\"хочу еще одну попытку\", \"repeat_quiz\"),\n",
        "        (\"какая погода сегодня\", \"unknown\"),\n",
        "        (\"расскажи шутку\", \"unknown\")\n",
        "    ]\n",
        "\n",
        "    if model is None:\n",
        "        logger.error(\"Модель не загружена\")\n",
        "        return\n",
        "\n",
        "    model.eval()\n",
        "    correct = 0\n",
        "    results = []\n",
        "\n",
        "    for text, expected in test_cases:\n",
        "        # Преобразование текста\n",
        "        tokens = re.findall(r'\\b\\w+\\b', text.lower())\n",
        "        indices = [vocab.get(token, 1) for token in tokens]\n",
        "\n",
        "        # Ограничение длины\n",
        "        max_len = 50\n",
        "        if len(indices) > max_len:\n",
        "            indices = indices[:max_len]\n",
        "            length = max_len\n",
        "        else:\n",
        "            length = len(indices)\n",
        "            indices += [0] * (max_len - len(indices))\n",
        "\n",
        "        input_tensor = torch.tensor([indices], dtype=torch.long).to(device)\n",
        "        length_tensor = torch.tensor([length], dtype=torch.long).to(device)\n",
        "\n",
        "        with torch.no_grad():\n",
        "            output = model(input_tensor, length_tensor)\n",
        "            probabilities = F.softmax(output, dim=1)\n",
        "            confidence, predicted_idx = torch.max(probabilities, 1)\n",
        "\n",
        "            # Преобразуем индекс в имя класса\n",
        "            predicted_idx = predicted_idx.item()\n",
        "            if predicted_idx < len(class_names):\n",
        "                predicted_label = class_names[predicted_idx]\n",
        "            else:\n",
        "                predicted_label = f\"UNKNOWN_{predicted_idx}\"\n",
        "\n",
        "            is_correct = predicted_label == expected\n",
        "\n",
        "            if is_correct:\n",
        "                correct += 1\n",
        "\n",
        "            status = \"✓\" if is_correct else \"✗\"\n",
        "            result_str = f\"'{text[:30]:30} → '{predicted_label:20}' (idx: {predicted_idx:2d}, conf: {confidence.item():6.2%}) {status}\"\n",
        "            results.append(result_str)\n",
        "            logger.info(f\"  {result_str}\")\n",
        "\n",
        "    accuracy = correct / len(test_cases)\n",
        "    logger.info(f\"Точность на тестах: {accuracy:.2%} ({correct}/{len(test_cases)})\")\n",
        "\n",
        "    # Сохраняем результаты тестирования\n",
        "    test_results_file = f\"training_logs/test_results_{datetime.now().strftime('%Y%m%d_%H%M%S')}.txt\"\n",
        "    with open(test_results_file, 'w', encoding='utf-8') as f:\n",
        "        f.write(\"РЕЗУЛЬТАТЫ ТЕСТИРОВАНИЯ МОДЕЛИ\\n\")\n",
        "        f.write(\"=\" * 70 + \"\\n\\n\")\n",
        "        for result in results:\n",
        "            f.write(result + \"\\n\")\n",
        "        f.write(f\"\\nИтоговая точность: {accuracy:.2%} ({correct}/{len(test_cases)})\\n\")\n",
        "\n",
        "    return accuracy\n",
        "\n",
        "def analyze_predictions(model, val_loader, label_encoder, device='cpu', num_examples=10):\n",
        "    \"\"\"Анализ конкретных предсказаний модели\"\"\"\n",
        "    logger.info(\"Анализ предсказаний модели...\")\n",
        "\n",
        "    model.eval()\n",
        "    examples_analyzed = 0\n",
        "    analysis_results = []\n",
        "\n",
        "    with torch.no_grad():\n",
        "        for batch in val_loader:\n",
        "            texts = batch['text'].to(device)\n",
        "            labels = batch['label'].to(device)\n",
        "            lengths = batch['length'].to(device)\n",
        "\n",
        "            outputs = model(texts, lengths)\n",
        "            probabilities = F.softmax(outputs, dim=1)\n",
        "            confidences, predicted = torch.max(probabilities, 1)\n",
        "\n",
        "            # Проходим по батчу\n",
        "            for i in range(len(texts)):\n",
        "                if examples_analyzed >= num_examples:\n",
        "                    break\n",
        "\n",
        "                # Получаем топ-3 предсказания\n",
        "                top_probs, top_indices = torch.topk(probabilities[i], 3)\n",
        "\n",
        "                true_label_idx = labels[i].item()\n",
        "                true_label = label_encoder.inverse_transform([true_label_idx])[0]\n",
        "                predicted_label = label_encoder.inverse_transform([predicted[i].item()])[0]\n",
        "\n",
        "                # Топ-3 предсказания\n",
        "                top_labels = []\n",
        "                for prob, idx in zip(top_probs, top_indices):\n",
        "                    label = label_encoder.inverse_transform([idx.item()])[0]\n",
        "                    top_labels.append(f\"'{label}': {prob.item():.2%}\")\n",
        "\n",
        "                example_info = {\n",
        "                    'text': texts[i][:50].tolist() if hasattr(texts[i], 'tolist') else str(texts[i][:50]),\n",
        "                    'true_label': true_label,\n",
        "                    'predicted_label': predicted_label,\n",
        "                    'confidence': confidences[i].item(),\n",
        "                    'top_predictions': top_labels,\n",
        "                    'correct': true_label == predicted_label\n",
        "                }\n",
        "\n",
        "                analysis_results.append(example_info)\n",
        "\n",
        "                logger.info(f\"\\n   Пример {examples_analyzed + 1}:\")\n",
        "                logger.info(f\"      Текст: {example_info['text']}...\")\n",
        "                logger.info(f\"      Истинная метка: '{true_label}'\")\n",
        "                logger.info(f\"      Предсказано: '{predicted_label}' ({confidences[i].item():.2%})\")\n",
        "                logger.info(f\"      Топ-3: {', '.join(top_labels)}\")\n",
        "                logger.info(f\"      {'✓ Правильно' if example_info['correct'] else '✗ Ошибка'}\")\n",
        "\n",
        "                examples_analyzed += 1\n",
        "\n",
        "            if examples_analyzed >= num_examples:\n",
        "                break\n",
        "\n",
        "    # Сохраняем анализ в файл\n",
        "    analysis_file = f\"training_logs/prediction_analysis_{datetime.now().strftime('%Y%m%d_%H%M%S')}.json\"\n",
        "    with open(analysis_file, 'w', encoding='utf-8') as f:\n",
        "        json.dump(analysis_results, f, ensure_ascii=False, indent=2)\n",
        "\n",
        "    logger.info(f\"Анализ предсказаний сохранен: {analysis_file}\")\n",
        "    return analysis_results\n",
        "\n",
        "# ============================================\n",
        "#  ОСНОВНАЯ ФУНКЦИЯ ОБУЧЕНИЯ\n",
        "# ============================================\n",
        "\n",
        "def main():\n",
        "    \"\"\"Основная функция обучения\"\"\"\n",
        "    logger.info(\"=\"*70)\n",
        "    logger.info(\"ОБУЧЕНИЕ НЕЙРОСЕТИ ДЛЯ КЛАССИФИКАЦИИ НАМЕРЕНИЙ\")\n",
        "    logger.info(\"=\"*70)\n",
        "\n",
        "    # Загрузка данных (ВНИМАНИЕ: теперь labels - строковые названия!)\n",
        "    texts, labels, class_names = load_intents_data()\n",
        "    if texts is None:\n",
        "        logger.error(\"Не удалось загрузить данные. Завершение работы.\")\n",
        "        return\n",
        "\n",
        "    logger.info(f\"\\nПример данных:\")\n",
        "    for i in range(min(5, len(texts))):\n",
        "        logger.info(f\"  Текст: '{texts[i][:50]}...', Метка: '{labels[i]}'\")\n",
        "\n",
        "    # Создание словаря\n",
        "    vocab = create_vocab(texts, min_freq=2)\n",
        "\n",
        "    if len(vocab) < 10:\n",
        "        logger.error(\"Слишком маленький словарь. Увеличьте количество данных.\")\n",
        "        return\n",
        "\n",
        "    # Кодирование меток (преобразуем строки в числа)\n",
        "    label_encoder = LabelEncoder()\n",
        "    labels_encoded = label_encoder.fit_transform(labels)\n",
        "\n",
        "    logger.info(f\"\\nМаппинг классов:\")\n",
        "    for i, class_name in enumerate(label_encoder.classes_):\n",
        "        count = sum(1 for label in labels_encoded if label == i)\n",
        "        logger.info(f\"  • {i:2d} → '{class_name:20}' ({count:4} примеров, {count/len(labels_encoded):.1%})\")\n",
        "\n",
        "    # Разделение данных\n",
        "    X_train, X_val, y_train, y_val = train_test_split(\n",
        "        texts, labels_encoded, test_size=0.2, random_state=42, stratify=labels_encoded\n",
        "    )\n",
        "\n",
        "    logger.info(f\"\\nРазделение данных:\")\n",
        "    logger.info(f\"  • Обучающая выборка: {len(X_train)} примеров\")\n",
        "    logger.info(f\"  • Валидационная выборка: {len(X_val)} примеров\")\n",
        "\n",
        "    # Создание DataLoader\n",
        "    train_dataset = IntentDataset(X_train, y_train, vocab)\n",
        "    val_dataset = IntentDataset(X_val, y_val, vocab)\n",
        "\n",
        "    train_loader = DataLoader(train_dataset, batch_size=32, shuffle=True)\n",
        "    val_loader = DataLoader(val_dataset, batch_size=32, shuffle=False)\n",
        "\n",
        "    # Инициализация модели\n",
        "    device = torch.device('cuda' if torch.cuda.is_available() else 'cpu')\n",
        "    logger.info(f\"\\nНастройки обучения:\")\n",
        "    logger.info(f\"  • Устройство: {device}\")\n",
        "    logger.info(f\"  • Размер словаря: {len(vocab)}\")\n",
        "    logger.info(f\"  • Количество классов: {len(label_encoder.classes_)}\")\n",
        "\n",
        "    try:\n",
        "        model = EnhancedIntentClassifier(\n",
        "            vocab_size=len(vocab),\n",
        "            embedding_dim=128,\n",
        "            hidden_dim=256,\n",
        "            output_dim=len(label_encoder.classes_),\n",
        "            num_layers=2,\n",
        "            dropout=0.3\n",
        "        ).to(device)\n",
        "\n",
        "        logger.info(f\"  • Параметры модели: {sum(p.numel() for p in model.parameters()):,}\")\n",
        "        logger.info(\"  Модель успешно инициализирована\")\n",
        "\n",
        "    except Exception as e:\n",
        "        logger.error(f\"Ошибка при создании модели: {e}\")\n",
        "        import traceback\n",
        "        traceback.print_exc()\n",
        "        return\n",
        "\n",
        "    # Обучение\n",
        "    try:\n",
        "        train_losses, train_accs, val_accs = train_model(\n",
        "            model, train_loader, val_loader, vocab, label_encoder,\n",
        "            num_epochs=20, device=device\n",
        "        )\n",
        "\n",
        "        # Полная оценка\n",
        "        evaluate_full_model(model, val_loader, label_encoder, device)\n",
        "\n",
        "        # Анализ предсказаний\n",
        "        analyze_predictions(model, val_loader, label_encoder, device)\n",
        "\n",
        "        # Тестирование на примерах\n",
        "        test_model_on_examples(model, vocab, label_encoder, device)\n",
        "\n",
        "        # Визуализация\n",
        "        plot_training_results(train_losses, train_accs, val_accs)\n",
        "\n",
        "    except Exception as e:\n",
        "        logger.error(f\"Ошибка во время обучения: {e}\")\n",
        "        import traceback\n",
        "        traceback.print_exc()\n",
        "        return\n",
        "\n",
        "    logger.info(\"\\n\" + \"=\"*70)\n",
        "    logger.info(\"ОБУЧЕНИЕ ЗАВЕРШЕНО!\")\n",
        "    logger.info(\"=\"*70)\n",
        "    logger.info(f\"Сохраненные файлы:\")\n",
        "    logger.info(f\"  • intent_classifier.pth - обученная модель\")\n",
        "    logger.info(f\"  • model_checkpoints/ - чекпоинты модели\")\n",
        "    logger.info(f\"  • training_logs/ - все логи обучения\")\n",
        "    logger.info(f\"  • training_results.png - графики обучения\")\n",
        "    logger.info(f\"  • confusion_matrix.png - матрица ошибок\")\n",
        "    logger.info(f\"  • extended_intents.json - данные для обучения\")\n",
        "    logger.info(f\"\\nМодель готова к использованию в боте!\")\n",
        "\n",
        "# ============================================\n",
        "#  ДОПОЛНИТЕЛЬНЫЕ ФУНКЦИИ\n",
        "# ============================================\n",
        "\n",
        "def test_existing_model():\n",
        "    \"\"\"Тестирование существующей модели\"\"\"\n",
        "    logger.info(\"\\n\" + \"=\"*70)\n",
        "    logger.info(\"ТЕСТИРОВАНИЕ СУЩЕСТВУЮЩЕЙ МОДЕЛИ\")\n",
        "    logger.info(\"=\"*70)\n",
        "\n",
        "    device = torch.device('cuda' if torch.cuda.is_available() else 'cpu')\n",
        "\n",
        "    # Попытка загрузить модель\n",
        "    model, vocab, label_encoder = load_model_from_checkpoint('intent_classifier.pth', device)\n",
        "\n",
        "    if model is None:\n",
        "        logger.error(\"Не удалось загрузить модель. Обучите новую модель.\")\n",
        "        return\n",
        "\n",
        "    # Тестирование модели\n",
        "    test_model_on_examples(model, vocab, label_encoder, device)\n",
        "\n",
        "    # Интерактивное тестирование\n",
        "    logger.info(\"\\nИнтерактивное тестирование (для выхода введите 'выход'):\")\n",
        "\n",
        "    test_log_file = f\"training_logs/interactive_test_{datetime.now().strftime('%Y%m%d_%H%M%S')}.txt\"\n",
        "    with open(test_log_file, 'w', encoding='utf-8') as f:\n",
        "        f.write(\"ИНТЕРАКТИВНОЕ ТЕСТИРОВАНИЕ МОДЕЛИ\\n\")\n",
        "        f.write(\"=\" * 70 + \"\\n\\n\")\n",
        "\n",
        "    while True:\n",
        "        user_input = input(\"\\nВведите фразу для классификации: \").strip()\n",
        "\n",
        "        if user_input.lower() in ['выход', 'exit', 'quit', 'q']:\n",
        "            logger.info(\"Завершение интерактивного тестирования\")\n",
        "            break\n",
        "\n",
        "        if not user_input:\n",
        "            continue\n",
        "\n",
        "        # Классификация\n",
        "        model.eval()\n",
        "        tokens = re.findall(r'\\b\\w+\\b', user_input.lower())\n",
        "        indices = [vocab.get(token, 1) for token in tokens]\n",
        "\n",
        "        max_len = 50\n",
        "        if len(indices) > max_len:\n",
        "            indices = indices[:max_len]\n",
        "            length = max_len\n",
        "        else:\n",
        "            length = len(indices)\n",
        "            indices += [0] * (max_len - len(indices))\n",
        "\n",
        "        input_tensor = torch.tensor([indices], dtype=torch.long).to(device)\n",
        "        length_tensor = torch.tensor([length], dtype=torch.long).to(device)\n",
        "\n",
        "        with torch.no_grad():\n",
        "            output = model(input_tensor, length_tensor)\n",
        "            probabilities = F.softmax(output, dim=1)\n",
        "\n",
        "            # Получаем топ-3 предсказания\n",
        "            top_probs, top_indices = torch.topk(probabilities, 3)\n",
        "\n",
        "            result_str = f\"\\nРезультаты для: '{user_input}'\"\n",
        "            result_str += \"\\n\" + \"-\" * 50\n",
        "\n",
        "            for i, (prob, idx) in enumerate(zip(top_probs[0], top_indices[0])):\n",
        "                label = label_encoder.inverse_transform([idx.item()])[0]\n",
        "                result_str += f\"\\n  {i+1}. '{label:20}' - {prob.item():.2%}\"\n",
        "\n",
        "            result_str += \"\\n\" + \"-\" * 50\n",
        "\n",
        "            print(result_str)\n",
        "            logger.info(f\"Интерактивный тест: {user_input} -> {label_encoder.inverse_transform([top_indices[0][0].item()])[0]}\")\n",
        "\n",
        "            # Записываем результат в лог\n",
        "            with open(test_log_file, 'a', encoding='utf-8') as f:\n",
        "                f.write(f\"Вход: '{user_input}'\\n\")\n",
        "                for i, (prob, idx) in enumerate(zip(top_probs[0], top_indices[0])):\n",
        "                    label = label_encoder.inverse_transform([idx.item()])[0]\n",
        "                    f.write(f\"  Топ-{i+1}: '{label}' ({prob.item():.2%})\\n\")\n",
        "                f.write(\"\\n\")\n",
        "\n",
        "    logger.info(f\"Лог интерактивного тестирования сохранен: {test_log_file}\")\n",
        "\n",
        "def quick_train():\n",
        "    \"\"\"Быстрое обучение с базовыми параметрами\"\"\"\n",
        "    logger.info(\"\\n\" + \"=\"*70)\n",
        "    logger.info(\"БЫСТРОЕ ОБУЧЕНИЕ МОДЕЛИ\")\n",
        "    logger.info(\"=\"*70)\n",
        "\n",
        "    main()\n",
        "\n",
        "def check_data_file():\n",
        "    \"\"\"Проверка файла с данными\"\"\"\n",
        "    logger.info(\"\\n\" + \"=\"*70)\n",
        "    logger.info(\"ПРОВЕРКА ФАЙЛА С ДАННЫМИ\")\n",
        "    logger.info(\"=\"*70)\n",
        "\n",
        "    if not os.path.exists('extended_intents.json'):\n",
        "        logger.error(\"Файл extended_intents.json не найден!\")\n",
        "        return\n",
        "\n",
        "    try:\n",
        "        with open('extended_intents.json', 'r', encoding='utf-8') as f:\n",
        "            intents = json.load(f)\n",
        "\n",
        "        logger.info(\"Файл успешно загружен\")\n",
        "        logger.info(\"Содержимое файла:\")\n",
        "        logger.info(\"-\" * 50)\n",
        "\n",
        "        for intent_name, intent_data in intents.items():\n",
        "            patterns = intent_data.get('patterns', [])\n",
        "            responses = intent_data.get('responses', [])\n",
        "            logger.info(f\"\\nИнтент: '{intent_name}'\")\n",
        "            logger.info(f\"  • Количество примеров: {len(patterns)}\")\n",
        "            logger.info(f\"  • Количество ответов: {len(responses)}\")\n",
        "\n",
        "            if patterns:\n",
        "                logger.info(f\"  • Примеры фраз:\")\n",
        "                for i, pattern in enumerate(patterns[:3]):\n",
        "                    logger.info(f\"      {i+1}. '{pattern}'\")\n",
        "                if len(patterns) > 3:\n",
        "                    logger.info(f\"      ... и еще {len(patterns) - 3} фраз\")\n",
        "\n",
        "            if responses:\n",
        "                logger.info(f\"  • Примеры ответов:\")\n",
        "                for i, response in enumerate(responses[:2]):\n",
        "                    logger.info(f\"      {i+1}. '{response}'\")\n",
        "                if len(responses) > 2:\n",
        "                    logger.info(f\"      ... и еще {len(responses) - 2} ответов\")\n",
        "\n",
        "        logger.info(\"-\" * 50)\n",
        "        logger.info(f\"\\nИтого:\")\n",
        "        logger.info(f\"  • Всего интентов: {len(intents)}\")\n",
        "        total_patterns = sum(len(data.get('patterns', [])) for data in intents.values())\n",
        "        logger.info(f\"  • Всего фраз: {total_patterns}\")\n",
        "\n",
        "        # Сохраняем информацию о данных в файл\n",
        "        data_info_file = f\"training_logs/data_info_{datetime.now().strftime('%Y%m%d_%H%M%S')}.txt\"\n",
        "        with open(data_info_file, 'w', encoding='utf-8') as f:\n",
        "            f.write(\"ИНФОРМАЦИЯ О ДАННЫХ ДЛЯ ОБУЧЕНИЯ\\n\")\n",
        "            f.write(\"=\" * 70 + \"\\n\\n\")\n",
        "            f.write(f\"Файл данных: extended_intents.json\\n\")\n",
        "            f.write(f\"Время проверки: {datetime.now().strftime('%Y-%m-%d %H:%M:%S')}\\n\\n\")\n",
        "            f.write(f\"Всего интентов: {len(intents)}\\n\")\n",
        "            f.write(f\"Всего фраз: {total_patterns}\\n\\n\")\n",
        "\n",
        "            for intent_name, intent_data in intents.items():\n",
        "                patterns = intent_data.get('patterns', [])\n",
        "                responses = intent_data.get('responses', [])\n",
        "                f.write(f\"\\nИнтент: '{intent_name}'\\n\")\n",
        "                f.write(f\"  Количество примеров: {len(patterns)}\\n\")\n",
        "                f.write(f\"  Количество ответов: {len(responses)}\\n\")\n",
        "\n",
        "        logger.info(f\"Информация о данных сохранена: {data_info_file}\")\n",
        "\n",
        "    except Exception as e:\n",
        "        logger.error(f\"Ошибка при чтении файла: {e}\")\n",
        "\n",
        "# ============================================\n",
        "# ЗАПУСК ПРОГРАММЫ\n",
        "# ============================================\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    logger.info(f\"Запуск программы: {datetime.now().strftime('%Y-%m-%d %H:%M:%S')}\")\n",
        "    logger.info(f\"Текущий лог-файл: {current_log_file}\")\n",
        "\n",
        "    print(\"\\n\" + \"=\"*70)\n",
        "    print(\"СИСТЕМА ОБУЧЕНИЯ НЕЙРОСЕТИ ДЛЯ КЛАССИФИКАЦИИ НАМЕРЕНИЙ\")\n",
        "    print(\"=\"*70)\n",
        "    print(f\"Логирование включено. Логи будут сохранены в training_logs/\")\n",
        "    print(f\"Текущий лог-файл: {current_log_file}\")\n",
        "\n",
        "    print(\"\\nВыберите действие:\")\n",
        "    print(\"1. Проверить файл с данными\")\n",
        "    print(\"2. Обучить новую модель\")\n",
        "    print(\"3. Протестировать существующую модель\")\n",
        "    print(\"4. Быстрое обучение\")\n",
        "\n",
        "    choice = input(\"\\nВведите номер (1-4): \").strip()\n",
        "\n",
        "    if choice == '1':\n",
        "        check_data_file()\n",
        "    elif choice == '2':\n",
        "        main()\n",
        "    elif choice == '3':\n",
        "        test_existing_model()\n",
        "    elif choice == '4':\n",
        "        quick_train()\n",
        "    else:\n",
        "        logger.warning(f\"Неверный выбор: {choice}. Запускаю проверку файла...\")\n",
        "        check_data_file()\n",
        "\n",
        "    logger.info(f\"Завершение программы: {datetime.now().strftime('%Y-%m-%d %H:%M:%S')}\")"
