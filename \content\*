    {
      "cell_type": "code",
      "source": [
        "!pip install pyTelegramBotAPI"
      ],
      "metadata": {
        "id": "IB_dyDV34FZ8"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import os\n",
        "import json\n",
        "import re\n",
        "import random\n",
        "import logging\n",
        "from datetime import datetime\n",
        "from collections import defaultdict\n",
        "import telebot\n",
        "from telebot import types\n",
        "import torch\n",
        "import torch.nn as nn\n",
        "import torch.nn.functional as F\n",
        "\n",
        "print(\"=\" * 70)\n",
        "print(\" TELEGRAM БОТ С НЕЙРОСЕТЬЮ ДЛЯ КВИЗОВ\")\n",
        "print(\"=\" * 70)\n",
        "\n",
        "# ============================================\n",
        "# НАСТРОЙКА ЛОГИРОВАНИЯ В ФАЙЛ И КОНСОЛЬ\n",
        "# ============================================\n",
        "\n",
        "def setup_logging():\n",
        "    \"\"\"Настройка логирования в файл и консоль\"\"\"\n",
        "    # Создаем директорию для логов, если её нет\n",
        "    log_dir = 'logs'\n",
        "    if not os.path.exists(log_dir):\n",
        "        os.makedirs(log_dir)\n",
        "\n",
        "    # Формируем имя файла с датой\n",
        "    current_date = datetime.now().strftime('%Y-%m-%d')\n",
        "    log_filename = f'{log_dir}/quiz_bot_{current_date}.log'\n",
        "\n",
        "    # Создаем форматтер для логов\n",
        "    log_format = '%(asctime)s - %(name)s - %(levelname)s - %(message)s'\n",
        "    date_format = '%Y-%m-%d %H:%M:%S'\n",
        "    formatter = logging.Formatter(log_format, date_format)\n",
        "\n",
        "    # Настройка корневого логгера\n",
        "    logger = logging.getLogger()\n",
        "    logger.setLevel(logging.INFO)\n",
        "\n",
        "    # Очищаем существующие обработчики\n",
        "    logger.handlers.clear()\n",
        "\n",
        "    # Файловый обработчик\n",
        "    file_handler = logging.FileHandler(log_filename, encoding='utf-8')\n",
        "    file_handler.setLevel(logging.INFO)\n",
        "    file_handler.setFormatter(formatter)\n",
        "    logger.addHandler(file_handler)\n",
        "\n",
        "    # Консольный обработчик\n",
        "    console_handler = logging.StreamHandler()\n",
        "    console_handler.setLevel(logging.INFO)\n",
        "    console_handler.setFormatter(formatter)\n",
        "    logger.addHandler(console_handler)\n",
        "\n",
        "    # Отдельный логгер для действий пользователей\n",
        "    user_logger = logging.getLogger('user_actions')\n",
        "    user_logger.setLevel(logging.INFO)\n",
        "\n",
        "    user_handler = logging.FileHandler(f'{log_dir}/user_actions_{current_date}.log', encoding='utf-8')\n",
        "    user_handler.setLevel(logging.INFO)\n",
        "    user_formatter = logging.Formatter('%(asctime)s - %(message)s', date_format)\n",
        "    user_handler.setFormatter(user_formatter)\n",
        "    user_logger.addHandler(user_handler)\n",
        "    user_logger.propagate = False  # Не пропускать в корневой логгер\n",
        "\n",
        "    # Логгер для ошибок\n",
        "    error_logger = logging.getLogger('errors')\n",
        "    error_logger.setLevel(logging.ERROR)\n",
        "\n",
        "    error_handler = logging.FileHandler(f'{log_dir}/errors_{current_date}.log', encoding='utf-8')\n",
        "    error_handler.setLevel(logging.ERROR)\n",
        "    error_handler.setFormatter(formatter)\n",
        "    error_logger.addHandler(error_handler)\n",
        "    error_logger.propagate = False\n",
        "\n",
        "    return logger, user_logger, error_logger\n",
        "\n",
        "# Инициализация логгеров\n",
        "logger, user_logger, error_logger = setup_logging()\n",
        "\n",
        "# ============================================\n",
        "#  КЛАСС ДЛЯ ЗАГРУЗКИ МОДЕЛИ (С АВТООПРЕДЕЛЕНИЕМ АРХИТЕКТУРЫ)\n",
        "# ============================================\n",
        "\n",
        "class NeuralNetworkPredictor:\n",
        "    def __init__(self, model_path='intent_classifier.pth'):\n",
        "        self.model = None\n",
        "        self.vocab = None\n",
        "        self.classes = None\n",
        "        self.device = torch.device('cuda' if torch.cuda.is_available() else 'cpu')\n",
        "        self.load_model(model_path)\n",
        "\n",
        "    def load_model(self, model_path):\n",
        "        \"\"\"Загрузка обученной модели с автоопределением архитектуры\"\"\"\n",
        "        if not os.path.exists(model_path):\n",
        "            logger.error(f\"Файл модели {model_path} не найден!\")\n",
        "            logger.info(\"Сначала обучите модель с помощью train_neural_network.py\")\n",
        "            return False\n",
        "\n",
        "        try:\n",
        "            checkpoint = torch.load(model_path, map_location=self.device)\n",
        "\n",
        "            # Получаем параметры из checkpoint\n",
        "            self.vocab = checkpoint.get('vocab', {})\n",
        "            self.classes = checkpoint.get('classes', [])\n",
        "\n",
        "            if not self.vocab:\n",
        "                logger.error(\"Словарь не найден в файле модели!\")\n",
        "                return False\n",
        "\n",
        "            if not self.classes:\n",
        "                logger.error(\"Классы не найдены в файле модели!\")\n",
        "                return False\n",
        "\n",
        "            # Получаем state_dict для анализа\n",
        "            state_dict = checkpoint['model_state_dict']\n",
        "\n",
        "            # АНАЛИЗИРУЕМ АРХИТЕКТУРУ СОХРАНЕННОЙ МОДЕЛИ\n",
        "            logger.info(\"Анализ архитектуры сохраненной модели...\")\n",
        "\n",
        "            # Определяем embedding_dim\n",
        "            embedding_dim = state_dict['embedding.weight'].shape[1]\n",
        "            logger.info(f\"   • embedding_dim: {embedding_dim}\")\n",
        "\n",
        "            # Определяем hidden_dim и num_layers по ключам LSTM\n",
        "            lstm_keys = [k for k in state_dict.keys() if 'lstm' in k]\n",
        "\n",
        "            # Ищем максимальный номер слоя LSTM\n",
        "            num_layers = 1  # по умолчанию\n",
        "            for key in lstm_keys:\n",
        "                if 'weight_ih_l' in key:\n",
        "                    # Извлекаем номер слоя из ключа\n",
        "                    try:\n",
        "                        layer_num = int(key.split('weight_ih_l')[1].split('_')[0])\n",
        "                        num_layers = max(num_layers, layer_num + 1)\n",
        "                    except:\n",
        "                        pass\n",
        "\n",
        "            # Определяем hidden_dim по первому слою LSTM\n",
        "            first_lstm_key = f'lstm.weight_ih_l0'\n",
        "            if first_lstm_key in state_dict:\n",
        "                hidden_dim = state_dict[first_lstm_key].shape[0] // 4  # LSTM имеет 4 gate'а\n",
        "                logger.info(f\"   • hidden_dim: {hidden_dim}\")\n",
        "                logger.info(f\"   • num_layers: {num_layers}\")\n",
        "            else:\n",
        "                hidden_dim = 128\n",
        "                logger.warning(f\"Не удалось определить hidden_dim, используем {hidden_dim}\")\n",
        "\n",
        "            # Определяем output_dim\n",
        "            output_key = None\n",
        "            for key in state_dict.keys():\n",
        "                if 'fc_layers' in key and 'weight' in key:\n",
        "                    # Берем последний слой fc_layers\n",
        "                    output_key = key\n",
        "\n",
        "            if output_key:\n",
        "                output_dim = state_dict[output_key].shape[0]\n",
        "                logger.info(f\"   • output_dim: {output_dim}\")\n",
        "            else:\n",
        "                output_dim = len(self.classes)\n",
        "                logger.warning(f\"Не удалось определить output_dim, используем {output_dim}\")\n",
        "\n",
        "            # Определяем dropout (проверяем наличие dropout слоев)\n",
        "            dropout = 0.5  # значение по умолчанию\n",
        "\n",
        "            # СОЗДАЕМ АРХИТЕКТУРУ НА ОСНОВЕ ОПРЕДЕЛЕННЫХ ПАРАМЕТРОВ\n",
        "            class DynamicIntentClassifier(nn.Module):\n",
        "                def __init__(self, vocab_size, embedding_dim, hidden_dim, output_dim, num_layers, dropout):\n",
        "                    super().__init__()\n",
        "\n",
        "                    # Embedding слой\n",
        "                    self.embedding = nn.Embedding(vocab_size, embedding_dim, padding_idx=0)\n",
        "                    self.embedding_dropout = nn.Dropout(dropout)\n",
        "\n",
        "                    # BiLSTM с динамическим числом слоев\n",
        "                    self.lstm = nn.LSTM(\n",
        "                        embedding_dim,\n",
        "                        hidden_dim,\n",
        "                        num_layers=num_layers,\n",
        "                        bidirectional=True,\n",
        "                        batch_first=True,\n",
        "                        dropout=dropout if num_layers > 1 else 0\n",
        "                    )\n",
        "\n",
        "                    # Attention механизм\n",
        "                    self.attention = nn.Sequential(\n",
        "                        nn.Linear(hidden_dim * 2, hidden_dim),\n",
        "                        nn.Tanh(),\n",
        "                        nn.Linear(hidden_dim, 1)\n",
        "                    )\n",
        "\n",
        "                    # Полносвязные слои\n",
        "                    self.fc_layers = nn.Sequential(\n",
        "                        nn.Linear(hidden_dim * 2, hidden_dim),\n",
        "                        nn.BatchNorm1d(hidden_dim),\n",
        "                        nn.ReLU(),\n",
        "                        nn.Dropout(dropout),\n",
        "                        nn.Linear(hidden_dim, hidden_dim // 2),\n",
        "                        nn.BatchNorm1d(hidden_dim // 2),\n",
        "                        nn.ReLU(),\n",
        "                        nn.Dropout(dropout * 0.8),\n",
        "                        nn.Linear(hidden_dim // 2, output_dim)\n",
        "                    )\n",
        "\n",
        "                    self.dropout_rate = dropout\n",
        "\n",
        "                def forward(self, text, lengths=None):\n",
        "                    # Embedding\n",
        "                    embedded = self.embedding(text)\n",
        "                    embedded = self.embedding_dropout(embedded)\n",
        "\n",
        "                    # Packed sequence для LSTM\n",
        "                    if lengths is not None:\n",
        "                        packed_input = nn.utils.rnn.pack_padded_sequence(\n",
        "                            embedded, lengths.cpu(), batch_first=True, enforce_sorted=False\n",
        "                        )\n",
        "                        packed_output, (hidden, cell) = self.lstm(packed_input)\n",
        "                        output, _ = nn.utils.rnn.pad_packed_sequence(packed_output, batch_first=True)\n",
        "                    else:\n",
        "                        output, (hidden, cell) = self.lstm(embedded)\n",
        "\n",
        "                    # Attention\n",
        "                    attention_weights = F.softmax(self.attention(output), dim=1)\n",
        "                    context = torch.sum(attention_weights * output, dim=1)\n",
        "\n",
        "                    # Дополнительный dropout\n",
        "                    context = F.dropout(context, p=self.dropout_rate * 0.5, training=self.training)\n",
        "\n",
        "                    # Классификация\n",
        "                    logits = self.fc_layers(context)\n",
        "\n",
        "                    return logits\n",
        "\n",
        "            # Создаем модель с определенными параметрами\n",
        "            self.model = DynamicIntentClassifier(\n",
        "                vocab_size=len(self.vocab),\n",
        "                embedding_dim=embedding_dim,\n",
        "                hidden_dim=hidden_dim,\n",
        "                output_dim=output_dim,\n",
        "                num_layers=num_layers,\n",
        "                dropout=dropout\n",
        "            ).to(self.device)\n",
        "\n",
        "            # Загружаем веса\n",
        "            try:\n",
        "                self.model.load_state_dict(state_dict)\n",
        "                logger.info(\"Модель успешно загружена (полное соответствие)\")\n",
        "            except Exception as e:\n",
        "                logger.warning(f\"Частичная загрузка модели: {e}\")\n",
        "                # Пробуем нестрогую загрузку\n",
        "                model_dict = self.model.state_dict()\n",
        "\n",
        "                # Загружаем совпадающие ключи\n",
        "                pretrained_dict = {k: v for k, v in state_dict.items()\n",
        "                                 if k in model_dict and model_dict[k].shape == v.shape}\n",
        "\n",
        "                # Показываем статус загрузки\n",
        "                loaded_keys = list(pretrained_dict.keys())\n",
        "                total_keys = len(model_dict)\n",
        "\n",
        "                logger.info(f\"Загружено {len(loaded_keys)}/{total_keys} параметров\")\n",
        "\n",
        "                # Обновляем веса модели\n",
        "                model_dict.update(pretrained_dict)\n",
        "                self.model.load_state_dict(model_dict, strict=False)\n",
        "\n",
        "            self.model.eval()\n",
        "\n",
        "            accuracy = checkpoint.get('accuracy', 0)\n",
        "            logger.info(f\"Модель успешно инициализирована\")\n",
        "            logger.info(f\"   • Точность: {accuracy:.2%}\")\n",
        "            logger.info(f\"   • Архитектура: BiLSTM x{num_layers} (hidden={hidden_dim})\")\n",
        "            logger.info(f\"   • Словарь: {len(self.vocab)} слов\")\n",
        "            logger.info(f\"   • Классы: {len(self.classes)}\")\n",
        "\n",
        "            # Тестовый прогноз для проверки\n",
        "            try:\n",
        "                test_text = \"привет\"\n",
        "                tokens = re.findall(r'\\b\\w+\\b', test_text.lower())\n",
        "                indices = [self.vocab.get(token, 1) for token in tokens]\n",
        "                indices = indices[:50] + [0] * (50 - len(indices))\n",
        "                input_tensor = torch.tensor([indices], dtype=torch.long).to(self.device)\n",
        "                with torch.no_grad():\n",
        "                    _ = self.model(input_tensor)\n",
        "                logger.info(\"Тестовый прогноз выполнен успешно\")\n",
        "            except Exception as e:\n",
        "                logger.warning(f\"Тестовый прогноз не удался: {e}\")\n",
        "\n",
        "            return True\n",
        "\n",
        "        except Exception as e:\n",
        "            logger.error(f\"Ошибка загрузки модели: {e}\")\n",
        "            error_logger.error(f\"Ошибка загрузки модели: {e}\")\n",
        "            import traceback\n",
        "            traceback.print_exc()\n",
        "            return False\n",
        "\n",
        "    def predict_intent(self, text):\n",
        "        \"\"\"Предсказание намерения\"\"\"\n",
        "        if self.model is None or self.vocab is None:\n",
        "            logger.warning(\"Модель не загружена, используется fallback классификация\")\n",
        "            return self.fallback_prediction(text)\n",
        "\n",
        "        try:\n",
        "            # Токенизация\n",
        "            tokens = re.findall(r'\\b\\w+\\b', text.lower())\n",
        "            indices = [self.vocab.get(token, 1) for token in tokens]\n",
        "\n",
        "            # Ограничение длины\n",
        "            max_len = 50\n",
        "            if len(indices) > max_len:\n",
        "                indices = indices[:max_len]\n",
        "                length = max_len\n",
        "            else:\n",
        "                length = len(indices)\n",
        "                indices += [0] * (max_len - len(indices))\n",
        "\n",
        "            # Преобразование в тензор\n",
        "            input_tensor = torch.tensor([indices], dtype=torch.long).to(self.device)\n",
        "            length_tensor = torch.tensor([length], dtype=torch.long).to(self.device)\n",
        "\n",
        "            # Предсказание\n",
        "            with torch.no_grad():\n",
        "                output = self.model(input_tensor, length_tensor)\n",
        "                probabilities = F.softmax(output, dim=1)\n",
        "                confidence, predicted_idx = torch.max(probabilities, 1)\n",
        "\n",
        "                # Если уверенность низкая, используем keyword-based классификацию\n",
        "                confidence_threshold = 0.4  # Можно настроить\n",
        "                if confidence.item() < confidence_threshold:\n",
        "                    logger.info(f\"Низкая уверенность ({confidence.item():.2%}), используем keyword классификацию\")\n",
        "                    return self.keyword_prediction(text)\n",
        "\n",
        "                intent_name = self.classes[predicted_idx.item()]\n",
        "\n",
        "                logger.info(f\"Предсказание: {intent_name} (уверенность: {confidence.item():.2%})\")\n",
        "                return {\n",
        "                    'intent': intent_name,\n",
        "                    'confidence': confidence.item(),\n",
        "                    'method': 'neural'\n",
        "                }\n",
        "\n",
        "        except Exception as e:\n",
        "            logger.error(f\"Ошибка предсказания: {e}\")\n",
        "            error_logger.error(f\"Ошибка предсказания для текста '{text}': {e}\")\n",
        "            return self.keyword_prediction(text)\n",
        "\n",
        "    def keyword_prediction(self, text):\n",
        "        \"\"\"Резервная классификация по ключевым словам\"\"\"\n",
        "        text_lower = text.lower()\n",
        "\n",
        "        keyword_mapping = {\n",
        "            'greeting': ['привет', 'здравствуй', 'добрый', 'хай', 'hello', 'hi', 'здарова', 'доброе', 'доброго'],\n",
        "            'start_quiz': ['начать', 'квиз', 'играть', 'старт', 'викторина', 'сыграть', 'игру', 'начнем', 'запусти', 'поиграть'],\n",
        "            'difficulty': ['легк', 'средн', 'сложн', 'уровен', 'новичок', 'простой', 'эксперт', 'сложност', 'уровень'],\n",
        "            'statistics': ['статистика', 'результат', 'счет', 'рекорд', 'история', 'прогресс', 'очков', 'показатель', 'рекорды'],\n",
        "            'help': ['помощь', 'правило', 'инструкция', 'как играть', 'руководство', 'объясни', 'помоги', 'подскажи', 'объяснение'],\n",
        "            'goodbye': ['пока', 'до свидани', 'выход', 'стоп', 'bye', 'закончить', 'прощай', 'всего', 'удачи', 'досвидания'],\n",
        "            'answer_feedback': ['правильн', 'неправильн', 'верн', 'ошибк', 'точно', 'корректно', 'верный', 'неверный'],\n",
        "            'repeat_quiz': ['еще раз', 'хочу еще', 'повтори', 'снова', 'еще одну', 'заново', 'еще раунд', 'повторить'],\n",
        "            'unknown': ['погода', 'шутка', 'анекдот', 'время', 'который час', 'что делаешь', 'как жизнь', 'сегодня']\n",
        "        }\n",
        "\n",
        "        scores = {}\n",
        "        for intent, keywords in keyword_mapping.items():\n",
        "            score = 0\n",
        "            for keyword in keywords:\n",
        "                if keyword in text_lower:\n",
        "                    score += 1\n",
        "            if score > 0:\n",
        "                scores[intent] = score\n",
        "\n",
        "        if scores:\n",
        "            best_intent = max(scores, key=scores.get)\n",
        "            confidence = min(scores[best_intent] / 3, 0.8)\n",
        "            logger.info(f\"Keyword классификация: {best_intent} (оценка: {scores[best_intent]})\")\n",
        "            return {'intent': best_intent, 'confidence': confidence, 'method': 'keyword'}\n",
        "\n",
        "        logger.info(\"Не удалось классифицировать, используется unknown\")\n",
        "        return {'intent': 'unknown', 'confidence': 0, 'method': 'keyword'}\n",
        "\n",
        "    def fallback_prediction(self, text):\n",
        "        \"\"\"Базовая классификация\"\"\"\n",
        "        text_lower = text.lower()\n",
        "\n",
        "        if any(word in text_lower for word in ['привет', 'здравствуй', 'хай', 'добрый', 'здрасьте']):\n",
        "            return {'intent': 'greeting', 'confidence': 0.7, 'method': 'fallback'}\n",
        "        elif any(word in text_lower for word in ['начать', 'играть', 'квиз', 'викторина', 'старт', 'игру']):\n",
        "            return {'intent': 'start_quiz', 'confidence': 0.7, 'method': 'fallback'}\n",
        "        elif any(word in text_lower for word in ['статистика', 'результат', 'счет', 'рекорд', 'очков']):\n",
        "            return {'intent': 'statistics', 'confidence': 0.6, 'method': 'fallback'}\n",
        "        elif any(word in text_lower for word in ['помощь', 'правила', 'инструкция', 'помоги', 'как играть']):\n",
        "            return {'intent': 'help', 'confidence': 0.6, 'method': 'fallback'}\n",
        "        elif any(word in text_lower for word in ['пока', 'до свидания', 'выход', 'стоп', 'закончить']):\n",
        "            return {'intent': 'goodbye', 'confidence': 0.7, 'method': 'fallback'}\n",
        "        elif any(word in text_lower for word in ['легк', 'средн', 'сложн', 'уровен', 'сложност']):\n",
        "            return {'intent': 'difficulty', 'confidence': 0.6, 'method': 'fallback'}\n",
        "\n",
        "        return {'intent': 'unknown', 'confidence': 0, 'method': 'fallback'}\n",
        "\n",
        "# ============================================\n",
        "# КЛАСС ДЛЯ ВОПРОСОВ\n",
        "# ============================================\n",
        "\n",
        "class QuizManager:\n",
        "    def __init__(self, questions_file='quiz_questions.json'):\n",
        "        self.questions_file = questions_file\n",
        "        self.questions = self.load_questions()\n",
        "        self.used_questions = defaultdict(set)\n",
        "        logger.info(f\"QuizManager инициализирован. Загружено вопросов: \"\n",
        "                   f\"легких={len(self.questions.get('легкий', []))}, \"\n",
        "                   f\"средних={len(self.questions.get('средний', []))}, \"\n",
        "                   f\"сложных={len(self.questions.get('сложный', []))}\")\n",
        "\n",
        "    def load_questions(self):\n",
        "        \"\"\"Загрузка вопросов из файла\"\"\"\n",
        "        default_questions = {\n",
        "            'легкий': [\n",
        "                {\n",
        "                    'question': 'Столица Франции?',\n",
        "                    'options': ['Лондон', 'Париж', 'Берлин', 'Мадрид'],\n",
        "                    'correct': 1,\n",
        "                    'points': 10,\n",
        "                    'explanation': 'Париж - столица и крупнейший город Франции.'\n",
        "                },\n",
        "                {\n",
        "                    'question': '2 + 2 = ?',\n",
        "                    'options': ['3', '4', '5', '6'],\n",
        "                    'correct': 1,\n",
        "                    'points': 5,\n",
        "                    'explanation': 'Базовое арифметическое действие.'\n",
        "                },\n",
        "                {\n",
        "                    'question': 'Сколько цветов у радуги?',\n",
        "                    'options': ['5', '6', '7', '8'],\n",
        "                    'correct': 2,\n",
        "                    'points': 10,\n",
        "                    'explanation': '7 цветов: красный, оранжевый, желтый, зеленый, голубой, синий, фиолетовый.'\n",
        "                },\n",
        "            ],\n",
        "            'средний': [\n",
        "                {\n",
        "                    'question': 'Год основания Москвы?',\n",
        "                    'options': ['1147', '1247', '1347', '1447'],\n",
        "                    'correct': 0,\n",
        "                    'points': 15,\n",
        "                    'explanation': 'Первое упоминание Москвы в летописи датируется 1147 годом.'\n",
        "                },\n",
        "                {\n",
        "                    'question': 'Автор романа \"Война и мир\"?',\n",
        "                    'options': ['Достоевский', 'Толстой', 'Чехов', 'Тургенев'],\n",
        "                    'correct': 1,\n",
        "                    'points': 15,\n",
        "                    'explanation': 'Лев Николаевич Толстой - автор эпопеи \"Война и мир\".'\n",
        "                },\n",
        "            ],\n",
        "            'сложный': [\n",
        "                {\n",
        "                    'question': 'Формула Эйнштейна для энергии?',\n",
        "                    'options': ['E=mgh', 'E=mc²', 'F=ma', 'PV=nRT'],\n",
        "                    'correct': 1,\n",
        "                    'points': 20,\n",
        "                    'explanation': 'E=mc² - знаменитая формула эквивалентности массы и энергии.'\n",
        "                },\n",
        "                {\n",
        "                    'question': 'Кто открыл пенициллин?',\n",
        "                    'options': ['Пастер', 'Флеминг', 'Кюри', 'Эйнштейн'],\n",
        "                    'correct': 1,\n",
        "                    'points': 20,\n",
        "                    'explanation': 'Александр Флеминг открыл пенициллин в 1928 году.'\n",
        "                },\n",
        "            ]\n",
        "        }\n",
        "\n",
        "        if os.path.exists(self.questions_file):\n",
        "            try:\n",
        "                with open(self.questions_file, 'r', encoding='utf-8') as f:\n",
        "                    questions = json.load(f)\n",
        "                logger.info(f\"Загружены вопросы из {self.questions_file}\")\n",
        "                return questions\n",
        "            except Exception as e:\n",
        "                logger.warning(f\"Ошибка загрузки вопросов: {e}, используются демо-вопросы\")\n",
        "                error_logger.warning(f\"Ошибка загрузки вопросов из {self.questions_file}: {e}\")\n",
        "        else:\n",
        "            logger.info(\"Файл с вопросами не найден, создаются демо-вопросы\")\n",
        "\n",
        "        # Создаем файл с демо-вопросами\n",
        "        try:\n",
        "            with open(self.questions_file, 'w', encoding='utf-8') as f:\n",
        "                json.dump(default_questions, f, ensure_ascii=False, indent=2)\n",
        "            logger.info(f\"Создан файл с демо-вопросами: {self.questions_file}\")\n",
        "        except Exception as e:\n",
        "            logger.warning(f\"Не удалось сохранить демо-вопросы: {e}\")\n",
        "            error_logger.warning(f\"Не удалось сохранить демо-вопросы: {e}\")\n",
        "\n",
        "        return default_questions\n",
        "\n",
        "    def get_questions(self, difficulty, user_id, num=5):\n",
        "        \"\"\"Получение вопросов для квиза\"\"\"\n",
        "        if difficulty not in self.questions:\n",
        "            logger.warning(f\"Запрошен неизвестный уровень сложности: {difficulty}, используется 'средний'\")\n",
        "            difficulty = 'средний'\n",
        "\n",
        "        available_questions = self.questions.get(difficulty, [])\n",
        "\n",
        "        # Фильтруем уже использованные вопросы\n",
        "        used = self.used_questions[user_id]\n",
        "        filtered = [q for q in available_questions\n",
        "                   if q['question'] not in used]\n",
        "\n",
        "        # Если использованы все вопросы, сбрасываем\n",
        "        if len(filtered) < num:\n",
        "            logger.info(f\"Сброс использованных вопросов для пользователя {user_id}. Было: {len(used)}\")\n",
        "            self.used_questions[user_id] = set()\n",
        "            filtered = available_questions.copy()\n",
        "\n",
        "        # Выбираем случайные вопросы\n",
        "        if len(filtered) <= num:\n",
        "            selected = filtered.copy()\n",
        "        else:\n",
        "            selected = random.sample(filtered, num)\n",
        "\n",
        "        # Помечаем как использованные\n",
        "        for q in selected:\n",
        "            self.used_questions[user_id].add(q['question'])\n",
        "\n",
        "        # Перемешиваем варианты ответов\n",
        "        for q in selected:\n",
        "            self.shuffle_options(q)\n",
        "\n",
        "        logger.debug(f\"Для пользователя {user_id} выбрано {len(selected)} вопросов уровня {difficulty}\")\n",
        "        return selected\n",
        "\n",
        "    def shuffle_options(self, question):\n",
        "        \"\"\"Перемешивание вариантов ответов\"\"\"\n",
        "        options = question['options'].copy()\n",
        "        correct_answer = options[question['correct']]\n",
        "        random.shuffle(options)\n",
        "        question['correct'] = options.index(correct_answer)\n",
        "        question['options'] = options\n",
        "\n",
        "# ============================================\n",
        "#  ОСНОВНОЙ КЛАСС БОТА\n",
        "# ============================================\n",
        "\n",
        "class TelegramQuizBot:\n",
        "    def __init__(self, token):\n",
        "        self.token = token\n",
        "        self.bot = telebot.TeleBot(token)\n",
        "        self.users = defaultdict(dict)\n",
        "\n",
        "        # Логирование старта инициализации\n",
        "        logger.info(\"=\" * 50)\n",
        "        logger.info(\"Инициализация Telegram бота для квизов\")\n",
        "        logger.info(\"=\" * 50)\n",
        "\n",
        "        # Загрузка компонентов\n",
        "        logger.info(\"Инициализация компонентов бота...\")\n",
        "        self.neural_network = NeuralNetworkPredictor()\n",
        "        self.quiz_manager = QuizManager()\n",
        "\n",
        "        # Загрузка интентов\n",
        "        self.intents = self.load_intents()\n",
        "\n",
        "        # Настройки\n",
        "        self.difficulty_levels = {\n",
        "            'легкий': {'questions': 5, 'multiplier': 1.0},\n",
        "            'средний': {'questions': 5, 'multiplier': 1.5},\n",
        "            'сложный': {'questions': 5, 'multiplier': 2.0}\n",
        "        }\n",
        "\n",
        "        # Установка обработчиков\n",
        "        self.setup_handlers()\n",
        "\n",
        "        if self.neural_network.model:\n",
        "            logger.info(\"Бот успешно инициализирован с нейросетью!\")\n",
        "        else:\n",
        "            logger.warning(\"Бот инициализирован без нейросети, используется keyword классификация\")\n",
        "\n",
        "        logger.info(\"Инициализация бота завершена успешно\")\n",
        "\n",
        "    def load_intents(self):\n",
        "        \"\"\"Загрузка намерений для ответов\"\"\"\n",
        "        intents_file = 'extended_intents.json'\n",
        "        if os.path.exists(intents_file):\n",
        "            try:\n",
        "                with open(intents_file, 'r', encoding='utf-8') as f:\n",
        "                    intents = json.load(f)\n",
        "                logger.info(f\"Загружены интенты из {intents_file}\")\n",
        "                return intents\n",
        "            except Exception as e:\n",
        "                logger.warning(f\"Не удалось загрузить {intents_file}: {e}\")\n",
        "                error_logger.warning(f\"Ошибка загрузки интентов: {e}\")\n",
        "\n",
        "        # Базовые ответы\n",
        "        logger.info(\"Используются стандартные ответы\")\n",
        "        return {\n",
        "            'greeting': {'responses': [\n",
        "                'Привет! Как дела?',\n",
        "                'Здравствуйте! Рад вас видеть!',\n",
        "                'Хай! Готовы к квизу?'\n",
        "            ]},\n",
        "            'start_quiz': {'responses': [\n",
        "                'Отлично! Давайте начнем квиз!',\n",
        "                'Начинаем викторину! Выберите уровень сложности.',\n",
        "                'Готов к вопросам! Какой уровень предпочитаете?'\n",
        "            ]},\n",
        "            'help': {'responses': [\n",
        "                'Я бот для викторин! Вот что я умею:\\n\\n'\n",
        "                '1. Проводить квизы разной сложности\\n'\n",
        "                '2. Вести статистику ваших игр\\n'\n",
        "                '3. Понимать естественную речь\\n\\n'\n",
        "                'Просто напишите, что хотите сделать!'\n",
        "            ]},\n",
        "            'goodbye': {'responses': ['До свидания!', 'Пока! Заходите еще!', 'Удачи!']},\n",
        "            'statistics': {'responses': ['Вот ваша статистика:']},\n",
        "            'difficulty': {'responses': ['Выберите уровень сложности:']},\n",
        "            'answer_feedback': {'responses': ['Спасибо за обратную связь!']},\n",
        "            'repeat_quiz': {'responses': ['Отлично! Давайте сыграем еще раз!']},\n",
        "            'unknown': {'responses': [\n",
        "                'Извините, я не совсем понял. Может, сыграем в квиз?',\n",
        "                'Пока я умею только проводить викторины. Хотите сыграть?'\n",
        "            ]}\n",
        "        }\n",
        "\n",
        "    def setup_handlers(self):\n",
        "        \"\"\"Настройка обработчиков сообщений\"\"\"\n",
        "\n",
        "        @self.bot.message_handler(commands=['start'])\n",
        "        def send_welcome(message):\n",
        "            user_id = message.from_user.id\n",
        "            username = message.from_user.first_name or message.from_user.username or \"друг\"\n",
        "\n",
        "            # Логирование нового пользователя\n",
        "            logger.info(f\"Новый пользователь: ID={user_id}, username={username}\")\n",
        "            user_logger.info(f\"НОВЫЙ ПОЛЬЗОВАТЕЛЬ: ID={user_id}, username={username}\")\n",
        "\n",
        "            # Инициализация пользователя\n",
        "            self.users[user_id] = {\n",
        "                'username': username,\n",
        "                'score': 0,\n",
        "                'games_played': 0,\n",
        "                'total_score': 0,\n",
        "                'best_score': 0,\n",
        "                'current_quiz': None,\n",
        "                'current_question': 0,\n",
        "                'difficulty': None,\n",
        "                'answers': []\n",
        "            }\n",
        "\n",
        "            markup = types.ReplyKeyboardMarkup(resize_keyboard=True, row_width=2)\n",
        "            markup.add('Начать квиз', 'Статистика')\n",
        "            markup.add('Помощь')\n",
        "\n",
        "            welcome_text = f\"\"\"\n",
        " Привет, {username}!\n",
        "\n",
        " Я бот КВИЗ!\n",
        "\n",
        " **Начать игру** - начать новый квиз\n",
        " **Статистика** - посмотреть свои результаты\n",
        " **Помощь** - узнать, как пользоваться ботом\n",
        "\n",
        " **Или просто напишите, что хотите сделать!**\n",
        "Например: \"Хочу начать игру\" или \"Покажи статистику\"\n",
        "            \"\"\"\n",
        "\n",
        "            self.bot.send_message(message.chat.id, welcome_text, reply_markup=markup)\n",
        "\n",
        "        @self.bot.message_handler(commands=['help'])\n",
        "        def send_help(message):\n",
        "            self.show_help(message.chat.id)\n",
        "\n",
        "        @self.bot.message_handler(commands=['stats', 'статистика'])\n",
        "        def send_stats(message):\n",
        "            self.show_statistics(message.from_user.id, message.chat.id)\n",
        "\n",
        "        @self.bot.message_handler(func=lambda message: True)\n",
        "        def handle_message(message):\n",
        "            self.process_message(message)\n",
        "\n",
        "        @self.bot.callback_query_handler(func=lambda call: True)\n",
        "        def handle_callback(call):\n",
        "            self.process_callback(call)\n",
        "\n",
        "    def process_message(self, message):\n",
        "        \"\"\"Обработка входящих сообщений\"\"\"\n",
        "        user_id = message.from_user.id\n",
        "        text = message.text\n",
        "\n",
        "        logger.info(f\"Сообщение от {user_id}: {text}\")\n",
        "        user_logger.info(f\"СООБЩЕНИЕ: ID={user_id}, text={text}\")\n",
        "\n",
        "        # Проверка специальных команд с кнопок\n",
        "        if text == 'Начать квиз':\n",
        "            self.start_quiz_flow(user_id, message.chat.id)\n",
        "            return\n",
        "        elif text == 'Статистика':\n",
        "            self.show_statistics(user_id, message.chat.id)\n",
        "            return\n",
        "        elif text == 'Помощь':\n",
        "            self.show_help(message.chat.id)\n",
        "            return\n",
        "\n",
        "        # Классификация намерения\n",
        "        intent_result = self.neural_network.predict_intent(text)\n",
        "        intent = intent_result['intent']\n",
        "\n",
        "        logger.info(f\"Намерение: {intent} (уверенность: {intent_result['confidence']:.2%}, метод: {intent_result['method']})\")\n",
        "\n",
        "        # Обработка намерения\n",
        "        if intent == 'start_quiz':\n",
        "            self.start_quiz_flow(user_id, message.chat.id)\n",
        "        elif intent == 'statistics':\n",
        "            self.show_statistics(user_id, message.chat.id)\n",
        "        elif intent == 'help':\n",
        "            self.show_help(message.chat.id)\n",
        "        elif intent == 'goodbye':\n",
        "            self.send_goodbye(message.chat.id)\n",
        "        elif intent == 'difficulty':\n",
        "            # Определение уровня сложности из текста\n",
        "            for level in ['легкий', 'средний', 'сложный']:\n",
        "                if level in text.lower():\n",
        "                    self.start_quiz_with_difficulty(user_id, message.chat.id, level)\n",
        "                    return\n",
        "            self.ask_difficulty(message.chat.id)\n",
        "        elif intent == 'greeting':\n",
        "            self.send_greeting(message.chat.id)\n",
        "        elif intent == 'repeat_quiz':\n",
        "            self.start_quiz_flow(user_id, message.chat.id)\n",
        "        else:\n",
        "            # Ответ из интентов\n",
        "            response = self.get_response(intent)\n",
        "            self.bot.send_message(message.chat.id, response)\n",
        "\n",
        "    def get_response(self, intent):\n",
        "        \"\"\"Получение ответа для намерения\"\"\"\n",
        "        if intent in self.intents and 'responses' in self.intents[intent]:\n",
        "            return random.choice(self.intents[intent]['responses'])\n",
        "\n",
        "        # Запасные ответы\n",
        "        responses = {\n",
        "            'greeting': \"Привет! Чем могу помочь?\",\n",
        "            'start_quiz': \"Давайте начнем квиз! Выберите уровень сложности:\",\n",
        "            'statistics': \"Сейчас покажу вашу статистику...\",\n",
        "            'help': \"Вот как пользоваться ботом:\",\n",
        "            'goodbye': \"До свидания! Возвращайтесь!\",\n",
        "            'difficulty': \"Пожалуйста, выберите уровень сложности:\",\n",
        "            'answer_feedback': \"Спасибо за обратную связь!\",\n",
        "            'repeat_quiz': \"Отлично! Давайте сыграем еще раз!\",\n",
        "            'unknown': \"Извините, не совсем понял. Хотите сыграть в квиз?\"\n",
        "        }\n",
        "\n",
        "        return responses.get(intent, \"Не могу найти подходящий ответ.\")\n",
        "\n",
        "    def start_quiz_flow(self, user_id, chat_id):\n",
        "        \"\"\"Начало процесса квиза\"\"\"\n",
        "        logger.info(f\"Пользователь {user_id} начал процесс квиза\")\n",
        "        user_logger.info(f\"НАЧАЛО КВИЗА: ID={user_id}\")\n",
        "        self.ask_difficulty(chat_id)\n",
        "\n",
        "    def ask_difficulty(self, chat_id):\n",
        "        \"\"\"Запрос уровня сложности\"\"\"\n",
        "        markup = types.ReplyKeyboardMarkup(resize_keyboard=True, row_width=3)\n",
        "        markup.add('Легкий', 'Средний', 'Сложный')\n",
        "        markup.add('Назад')\n",
        "\n",
        "        self.bot.send_message(\n",
        "            chat_id,\n",
        "            \"**Выберите уровень сложности:**\\n\\n\"\n",
        "            \"•**Легкий** - простые вопросы, 1x множитель очков\\n\"\n",
        "            \"•**Средний** - вопросы посложнее, 1.5x множитель\\n\"\n",
        "            \"•**Сложный** - сложные вопросы, 2x множитель\\n\\n\"\n",
        "            \"Нажмите кнопку с нужным уровнем\",\n",
        "            reply_markup=markup,\n",
        "            parse_mode='Markdown'\n",
        "        )\n",
        "\n",
        "    def start_quiz_with_difficulty(self, user_id, chat_id, difficulty):\n",
        "        \"\"\"Начало квиза с выбранной сложностью\"\"\"\n",
        "        if difficulty not in self.difficulty_levels:\n",
        "            logger.warning(f\"Пользователь {user_id} выбрал неизвестный уровень: {difficulty}\")\n",
        "            self.bot.send_message(chat_id, \"Неверный уровень сложности!\")\n",
        "            return\n",
        "\n",
        "        logger.info(f\"Пользователь {user_id} начал квиз с уровнем: {difficulty}\")\n",
        "        user_logger.info(f\"СТАРТ КВИЗА: ID={user_id}, уровень={difficulty}\")\n",
        "\n",
        "        # Получение вопросов\n",
        "        num_questions = self.difficulty_levels[difficulty]['questions']\n",
        "        questions = self.quiz_manager.get_questions(difficulty, user_id, num_questions)\n",
        "\n",
        "        if not questions:\n",
        "            logger.warning(f\"Нет доступных вопросов уровня {difficulty} для пользователя {user_id}\")\n",
        "            self.bot.send_message(chat_id, \"Нет доступных вопросов! Попробуйте другой уровень.\")\n",
        "            return\n",
        "\n",
        "        # Сохранение состояния\n",
        "        self.users[user_id]['current_quiz'] = questions\n",
        "        self.users[user_id]['current_question'] = 0\n",
        "        self.users[user_id]['score'] = 0\n",
        "        self.users[user_id]['difficulty'] = difficulty\n",
        "        self.users[user_id]['answers'] = []\n",
        "        self.users[user_id]['start_time'] = datetime.now()\n",
        "\n",
        "        logger.info(f\"Для пользователя {user_id} подготовлено {len(questions)} вопросов уровня {difficulty}\")\n",
        "\n",
        "        # Отправка первого вопроса\n",
        "        self.send_question(user_id, chat_id)\n",
        "\n",
        "    def send_question(self, user_id, chat_id):\n",
        "        \"\"\"Отправка текущего вопроса\"\"\"\n",
        "        user = self.users[user_id]\n",
        "        question_idx = user['current_question']\n",
        "        questions = user['current_quiz']\n",
        "\n",
        "        if question_idx >= len(questions):\n",
        "            self.finish_quiz(user_id, chat_id)\n",
        "            return\n",
        "\n",
        "        question = questions[question_idx]\n",
        "\n",
        "        # Создание клавиатуры с вариантами\n",
        "        markup = types.InlineKeyboardMarkup(row_width=2)\n",
        "        for i, option in enumerate(question['options']):\n",
        "            markup.add(types.InlineKeyboardButton(\n",
        "                text=f\"{chr(65+i)}. {option}\",  # A., B., C., D.\n",
        "                callback_data=str(i)\n",
        "            ))\n",
        "\n",
        "        question_text = f\"\"\"\n",
        " **Вопрос {question_idx + 1}/{len(questions)}**\n",
        "\n",
        " Уровень: {user['difficulty'].capitalize()}\n",
        " Счет: {user['score']} очков\n",
        "\n",
        "**{question['question']}**\n",
        "        \"\"\"\n",
        "\n",
        "        self.bot.send_message(chat_id, question_text, reply_markup=markup, parse_mode='Markdown')\n",
        "\n",
        "    def process_callback(self, call):\n",
        "        \"\"\"Обработка ответов на вопросы\"\"\"\n",
        "        user_id = call.from_user.id\n",
        "\n",
        "        if user_id not in self.users or not self.users[user_id]['current_quiz']:\n",
        "            logger.warning(f\"Попытка ответить на вопрос без активного квиза: {user_id}\")\n",
        "            self.bot.answer_callback_query(call.id, \"Квиз не активен! Начните новый.\")\n",
        "            return\n",
        "\n",
        "        user = self.users[user_id]\n",
        "        question_idx = user['current_question']\n",
        "        questions = user['current_quiz']\n",
        "\n",
        "        if question_idx >= len(questions):\n",
        "            self.bot.answer_callback_query(call.id, \"Квиз завершен!\")\n",
        "            return\n",
        "\n",
        "        question = questions[question_idx]\n",
        "        selected_answer = int(call.data)\n",
        "        is_correct = selected_answer == question['correct']\n",
        "\n",
        "        # Расчет очков\n",
        "        base_points = question['points']\n",
        "        multiplier = self.difficulty_levels[user['difficulty']]['multiplier']\n",
        "        points = int(base_points * multiplier) if is_correct else 0\n",
        "\n",
        "        # Логирование ответа\n",
        "        logger.info(f\"Пользователь {user_id} ответил на вопрос {question_idx+1}: \"\n",
        "                   f\"правильно={is_correct}, выбрано={selected_answer}, правильный={question['correct']}, очки={points}\")\n",
        "        user_logger.info(f\"ОТВЕТ: ID={user_id}, вопрос={question_idx+1}, правильно={is_correct}, очки={points}\")\n",
        "\n",
        "        if is_correct:\n",
        "            user['score'] += points\n",
        "            feedback = f\" **Правильно!** +{points} очков\\n\\n\"\n",
        "        else:\n",
        "            correct_option = question['options'][question['correct']]\n",
        "            feedback = f\" **Неправильно!** Правильный ответ: {correct_option}\\n\\n\"\n",
        "\n",
        "        # Добавляем объяснение если есть\n",
        "        if 'explanation' in question:\n",
        "            feedback += f\" {question['explanation']}\\n\\n\"\n",
        "\n",
        "        # Сохранение ответа\n",
        "        user['answers'].append({\n",
        "            'question': question['question'],\n",
        "            'selected': selected_answer,\n",
        "            'correct': is_correct,\n",
        "            'points': points\n",
        "        })\n",
        "\n",
        "        self.bot.send_message(call.message.chat.id, feedback, parse_mode='Markdown')\n",
        "        self.bot.answer_callback_query(call.id)\n",
        "\n",
        "        # Переход к следующему вопросу\n",
        "        user['current_question'] += 1\n",
        "\n",
        "        import time\n",
        "        time.sleep(1)\n",
        "\n",
        "        self.send_question(user_id, call.message.chat.id)\n",
        "\n",
        "    def finish_quiz(self, user_id, chat_id):\n",
        "        \"\"\"Завершение квиза\"\"\"\n",
        "        user = self.users[user_id]\n",
        "        start_time = user.get('start_time', datetime.now())\n",
        "        duration = datetime.now() - start_time\n",
        "\n",
        "        # Статистика\n",
        "        total_questions = len(user['current_quiz'])\n",
        "        correct_answers = sum(1 for a in user['answers'] if a['correct'])\n",
        "        accuracy = (correct_answers / total_questions) * 100 if total_questions > 0 else 0\n",
        "\n",
        "        # Обновление общей статистики\n",
        "        user['games_played'] += 1\n",
        "        user['total_score'] += user['score']\n",
        "\n",
        "        if user['score'] > user.get('best_score', 0):\n",
        "            user['best_score'] = user['score']\n",
        "\n",
        "        # Формирование результата\n",
        "        result_text = f\"\"\"\n",
        " **Квиз завершен!**\n",
        "\n",
        " **Результаты:**\n",
        "• Финальный счет: **{user['score']}** очков\n",
        "• Правильных ответов: **{correct_answers}/{total_questions}**\n",
        "• Точность: **{accuracy:.1f}%**\n",
        "• Уровень: **{user['difficulty'].capitalize()}**\n",
        "\n",
        "        \"\"\"\n",
        "\n",
        "        if accuracy >= 80:\n",
        "            result_text += \"\\n**Отличный результат! **\"\n",
        "        elif accuracy >= 60:\n",
        "            result_text += \"\\n**Хорошая работа! **\"\n",
        "        elif accuracy >= 40:\n",
        "            result_text += \"\\n**Неплохо! **\"\n",
        "        else:\n",
        "            result_text += \"\\n**Попробуйте еще раз! **\"\n",
        "\n",
        "        self.bot.send_message(chat_id, result_text, parse_mode='Markdown')\n",
        "\n",
        "        # Логирование результатов\n",
        "        logger.info(f\"Квиз завершен для пользователя {user_id}: \"\n",
        "                   f\"счет={user['score']}, правильно={correct_answers}/{total_questions}, \"\n",
        "                   f\"точность={accuracy:.1f}%, уровень={user['difficulty']}\")\n",
        "\n",
        "        user_logger.info(f\"ЗАВЕРШЕНИЕ КВИЗА: ID={user_id}, счет={user['score']}, \"\n",
        "                       f\"правильно={correct_answers}/{total_questions}, уровень={user['difficulty']}\")\n",
        "\n",
        "        # Сброс состояния\n",
        "        user['current_quiz'] = None\n",
        "        user['current_question'] = 0\n",
        "\n",
        "        # Возврат к основному меню\n",
        "        markup = types.ReplyKeyboardMarkup(resize_keyboard=True, row_width=2)\n",
        "        markup.add('Начать квиз', 'Статистика')\n",
        "        markup.add('Помощь')\n",
        "\n",
        "        self.bot.send_message(\n",
        "            chat_id,\n",
        "            \"Что хотите сделать дальше?\",\n",
        "            reply_markup=markup\n",
        "        )\n",
        "\n",
        "    def show_statistics(self, user_id, chat_id):\n",
        "        \"\"\"Показать статистику пользователя\"\"\"\n",
        "        user = self.users.get(user_id, {})\n",
        "\n",
        "        if not user or user.get('games_played', 0) == 0:\n",
        "            stats_text = \"\"\"\n",
        " **Ваша статистика:**\n",
        "\n",
        " Вы еще не играли.\n",
        " Начните свою первую игру!\n",
        "\n",
        "Нажмите \"Начать квиз\" или напишите \"хочу играть\"\n",
        "            \"\"\"\n",
        "        else:\n",
        "            avg_score = user['total_score'] / user['games_played'] if user['games_played'] > 0 else 0\n",
        "\n",
        "            stats_text = f\"\"\"\n",
        " **Ваша статистика:**\n",
        "\n",
        " Игр сыграно: **{user.get('games_played', 0)}**\n",
        " Лучший счет: **{user.get('best_score', 0)}** очков\n",
        " Средний счет: **{avg_score:.1f}** очков\n",
        " Всего очков: **{user.get('total_score', 0)}**\n",
        "\n",
        " Продолжайте в том же духе!\n",
        "            \"\"\"\n",
        "\n",
        "        self.bot.send_message(chat_id, stats_text, parse_mode='Markdown')\n",
        "        logger.info(f\"Пользователь {user_id} запросил статистику: игр={user.get('games_played', 0)}, \"\n",
        "                   f\"лучший счет={user.get('best_score', 0)}\")\n",
        "\n",
        "    def show_help(self, chat_id):\n",
        "        \"\"\"Показать справку\"\"\"\n",
        "        help_text = \"\"\"\n",
        "**Помощь по использованию бота:**\n",
        "\n",
        " **Как играть:**\n",
        "1. Нажмите \"Начать квиз\" или напишите \"хочу играть\"\n",
        "2. Выберите уровень сложности\n",
        "3. Отвечайте на вопросы, нажимая на кнопки\n",
        "4. Смотрите результаты\n",
        "\n",
        " **Что можно сказать:**\n",
        "• \"Привет\" / \"Здравствуйте\"\n",
        "• \"Хочу играть\" / \"Начать квиз\"\n",
        "• \"Легкий уровень\" / \"Сложный уровень\"\n",
        "• \"Покажи статистику\"\n",
        "• \"Помоги\"\n",
        "• \"Пока\"\n",
        "\n",
        " **Уровни сложности:**\n",
        "• Легкий: 1x множитель очков\n",
        "• Средний: 1.5x множитель очков\n",
        "• Сложный: 2x множитель очков\n",
        "\n",
        " **Кнопки быстрого доступа:**\n",
        " Начать квиз - начать новую игру\n",
        " Статистика - посмотреть результаты\n",
        " Помощь - показать это сообщение\n",
        "        \"\"\"\n",
        "\n",
        "        self.bot.send_message(chat_id, help_text, parse_mode='Markdown')\n",
        "\n",
        "    def send_greeting(self, chat_id):\n",
        "        \"\"\"Отправить приветствие\"\"\"\n",
        "        greetings = [\n",
        "            \"Привет! Рад вас видеть!\",\n",
        "            \"Здравствуйте! Как настроение?\",\n",
        "            \"Хай! Готовы к квизу?\",\n",
        "            \"Добрый день! Чем займемся?\"\n",
        "        ]\n",
        "        self.bot.send_message(chat_id, random.choice(greetings))\n",
        "\n",
        "    def send_goodbye(self, chat_id):\n",
        "        \"\"\"Попрощаться\"\"\"\n",
        "        goodbyes = [\n",
        "            \"До свидания! Заходите еще!\",\n",
        "            \"Пока! Жду вас снова!\",\n",
        "            \"Всего хорошего! Удачи!\",\n",
        "            \"Было приятно пообщаться! Возвращайтесь!\"\n",
        "        ]\n",
        "        self.bot.send_message(chat_id, random.choice(goodbyes))\n",
        "        logger.info(f\"Пользователь {chat_id} попрощался с ботом\")\n",
        "\n",
        "    def run(self):\n",
        "        \"\"\"Запуск бота\"\"\"\n",
        "        logger.info(\"=\" * 50)\n",
        "        logger.info(\"Запуск бота...\")\n",
        "        logger.info(\"=\" * 50)\n",
        "\n",
        "        print(\"\\n\" + \"=\"*70)\n",
        "        print(\"БОТ ЗАПУЩЕН И ГОТОВ К РАБОТЕ!\")\n",
        "        print(\"=\"*70)\n",
        "        print(\"\\n Информация:\")\n",
        "        print(f\"   • Нейросеть: {' Загружена' if self.neural_network.model else '  Keyword классификация'}\")\n",
        "        print(f\"   • Вопросы: {sum(len(q) for q in self.quiz_manager.questions.values())} всего\")\n",
        "        print(f\"   • Уровней: {len(self.difficulty_levels)}\")\n",
        "        print(f\"   • Логирование: включено (файлы в папке logs/)\")\n",
        "        print(\"\\n Напишите /start в Telegram для начала\")\n",
        "        print(\"=\"*70)\n",
        "\n",
        "        try:\n",
        "            logger.info(\"Бот начал polling...\")\n",
        "            self.bot.polling(none_stop=True, interval=0, timeout=20)\n",
        "        except Exception as e:\n",
        "            logger.error(f\"Ошибка при работе бота: {e}\")\n",
        "            error_logger.error(f\"КРИТИЧЕСКАЯ ОШИБКА БОТА: {e}\")\n",
        "            print(f\"\\n Ошибка: {e}\")\n",
        "\n",
        "# ============================================\n",
        "# ЗАПУСК БОТА\n",
        "# ============================================\n",
        "\n",
        "def get_telegram_token():\n",
        "    \"\"\"Получение токена Telegram бота\"\"\"\n",
        "    print(\"\\n\" + \"=\"*70)\n",
        "    print(\"ПОЛУЧЕНИЕ ТОКЕНА TELEGRAM БОТА\")\n",
        "    print(\"=\"*70)\n",
        "\n",
        "    # Попробуем считать из файла\n",
        "    try:\n",
        "        if os.path.exists('token.txt'):\n",
        "            with open('token.txt', 'r') as f:\n",
        "                token = f.read().strip()\n",
        "            if token:\n",
        "                print(f\" Используется токен из файла: {token[:10]}...\")\n",
        "                logger.info(f\"Используется токен из файла: {token[:10]}...\")\n",
        "                return token\n",
        "    except Exception as e:\n",
        "        logger.warning(f\"Ошибка чтения токена из файла: {e}\")\n",
        "\n",
        "    # Ввод с клавиатуры\n",
        "    try:\n",
        "        token = input(\"Введите токен Telegram бота: \").strip()\n",
        "        if token:\n",
        "            # Сохраним токен в файл для будущего использования\n",
        "            try:\n",
        "                with open('token.txt', 'w') as f:\n",
        "                    f.write(token)\n",
        "                print(f\" Токен сохранен в token.txt\")\n",
        "                logger.info(\"Токен сохранен в файл token.txt\")\n",
        "            except Exception as e:\n",
        "                logger.warning(f\"Не удалось сохранить токен в файл: {e}\")\n",
        "            print(f\" Токен получен: {token[:10]}...\")\n",
        "            return token\n",
        "    except Exception as e:\n",
        "        logger.error(f\"Ошибка ввода токена: {e}\")\n",
        "\n",
        "    print(\" Токен не получен.\")\n",
        "    logger.error(\"Токен Telegram не получен!\")\n",
        "    return None\n",
        "\n",
        "def main():\n",
        "    \"\"\"Основная функция запуска\"\"\"\n",
        "    print(\"\\n\" + \"=\"*70)\n",
        "    print(\" ЗАПУСК TELEGRAM БОТА\")\n",
        "    print(\"=\"*70)\n",
        "\n",
        "    # Проверка файлов\n",
        "    print(\"\\n Проверка файлов...\")\n",
        "\n",
        "    required_files = [\n",
        "        ('intent_classifier.pth', 'обученная модель (опционально)'),\n",
        "        ('extended_intents.json', 'данные намерений (опционально)'),\n",
        "        ('quiz_questions.json', 'вопросы для квиза')\n",
        "    ]\n",
        "\n",
        "    for filename, description in required_files:\n",
        "        if os.path.exists(filename):\n",
        "            print(f\"    {filename}\")\n",
        "        else:\n",
        "            print(f\"     {filename} (не найден)\")\n",
        "\n",
        "    # Получение токена\n",
        "    token = get_telegram_token()\n",
        "\n",
        "    if not token:\n",
        "        logger.error(\"Не удалось получить токен, бот не может быть запущен\")\n",
        "        return\n",
        "\n",
        "    # Запуск бота\n",
        "    try:\n",
        "        logger.info(\"Создание экземпляра бота...\")\n",
        "        bot = TelegramQuizBot(token)\n",
        "        logger.info(\"Запуск основного цикла бота...\")\n",
        "        bot.run()\n",
        "    except KeyboardInterrupt:\n",
        "        logger.info(\"Бот остановлен пользователем (Ctrl+C)\")\n",
        "        print(\"\\n Бот остановлен пользователем\")\n",
        "    except Exception as e:\n",
        "        logger.error(f\"Ошибка при запуске бота: {e}\")\n",
        "        error_logger.error(f\"ОШИБКА ЗАПУСКА БОТА: {e}\")\n",
        "        print(f\"\\n Ошибка при запуске бота: {e}\")\n",
        "        import traceback\n",
        "        traceback.print_exc()\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()"
